{
  "datasets": [
    {
      "name": "hpx_curr_trendline_dataset",
      "displayName": "HPX CURR Trendline Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('WEEK', week_active) as weekly_active, AVG(curr) as avg_curr FROM HPX_CURR GROUP BY DATE_TRUNC('WEEK', week_active) ORDER BY weekly_active"
      ]
    }
  ],
  "pages": [
    {
      "name": "hpx_curr_trendline_page",
      "displayName": "HPX CURR - Trendline",
      "layout": [
        {
          "widget": {
            "name": "hpx_curr_trendline_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_curr_trendline_dataset",
                  "fields": [
                    { "name": "weekly_active", "expression": "`weekly_active`" },
                    { "name": "avg_curr", "expression": "`avg_curr`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "weekly_active",
                  "displayName": "Week Active",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "avg_curr",
                  "displayName": "CURR",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "HPX CURR - Trendline",
                "showTitle": true,
                "subtitle": "% Devices active in the previous week t14t20, last week t7t13 and this week t0t6"
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}