{
  "datasets": [
    {
      "name": "hpx_curr_dataset",
      "displayName": "HPX CURR Dataset",
      "queryLines": [
        "SELECT week_active AS `week_active`, AVG(curr) AS `avg_curr` FROM team_css_analytics_prod.hpx_analytics.hpx_curr_metric_view GROUP BY week_active ORDER BY week_active"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_curr",
      "displayName": "CURR",
      "layout": [
        {
          "widget": {
            "name": "hpx_curr_trendline",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_curr_dataset",
                  "fields": [
                    {
                      "name": "week_active",
                      "expression": "`week_active`"
                    },
                    {
                      "name": "avg_curr",
                      "expression": "`avg_curr`"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "week_active",
                  "displayName": "Week Active",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fieldName": "avg_curr",
                  "displayName": "CURR",
                  "scale": {
                    "type": "quantitative"
                  }
                }
              },
              "frame": {
                "title": "HPX CURR - Trendline",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 6,
            "height": 6
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}