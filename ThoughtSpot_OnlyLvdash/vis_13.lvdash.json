{
  "datasets": [
    {
      "name": "hpx_first_use_behavior_flow_dataset",
      "displayName": "HPX First Use Behavior Flow Dataset",
      "queryLines": [
        "SELECT screen_1, screen_2, screen_3, screen_4, screen_5, SUM(devices) as total_devices FROM hpx_first_use_behavior_flow WHERE date >= '2023-05-01' AND screen_1 = 'welcome_S1' AND screen_2 != 'welcome_S2' AND screen_3 != 'welcome_S3' AND screen_4 != 'welcome_S4' AND screen_5 != 'welcome_S5' GROUP BY screen_1, screen_2, screen_3, screen_4, screen_5"
      ]
    }
  ],
  "pages": [
    {
      "name": "hpx_first_use_behavior_flow_page",
      "displayName": "HPX First Use Behavior Flow",
      "layout": [
        {
          "widget": {
            "name": "hpx_first_use_behavior_flow_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_first_use_behavior_flow_dataset",
                  "fields": [
                    { "name": "screen_1", "expression": "`screen_1`" },
                    { "name": "screen_2", "expression": "`screen_2`" },
                    { "name": "screen_3", "expression": "`screen_3`" },
                    { "name": "screen_4", "expression": "`screen_4`" },
                    { "name": "screen_5", "expression": "`screen_5`" },
                    { "name": "total_devices", "expression": "`total_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 1,
              "widgetType": "sankey",
              "encodings": {
                "value": { 
                  "fieldName": "total_devices", 
                  "displayName": "Total Devices" 
                },
                "stages": [
                  { "fieldName": "screen_1", "displayName": "Screen 1" },
                  { "fieldName": "screen_2", "displayName": "Screen 2" },
                  { "fieldName": "screen_3", "displayName": "Screen 3" },
                  { "fieldName": "screen_4", "displayName": "Screen 4" },
                  { "fieldName": "screen_5", "displayName": "Screen 5" }
                ]
              },
              "frame": {
                "title": "HPX First Use Behavior Flow",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}