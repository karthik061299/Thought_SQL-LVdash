{
  "datasets": [
    {
      "name": "sales_analytics_dataset",
      "displayName": "Sales Analytics Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('MONTH', order_date) as order_month, product_category, SUM(sales_amount) as total_sales, COUNT(order_id) as order_count, AVG(sales_amount) as avg_order_value FROM sales_schema.sales_fact_view WHERE order_date >= '2023-01-01' GROUP BY DATE_TRUNC('MONTH', order_date), product_category ORDER BY order_month, product_category"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_13",
      "displayName": "Sales Performance Dashboard",
      "layout": [
        {
          "widget": {
            "name": "sales_trend_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_analytics_dataset",
                  "fields": [
                    {
                      "name": "order_month",
                      "expression": "`order_month`"
                    },
                    {
                      "name": "product_category",
                      "expression": "`product_category`"
                    },
                    {
                      "name": "total_sales",
                      "expression": "`total_sales`"
                    },
                    {
                      "name": "order_count",
                      "expression": "`order_count`"
                    },
                    {
                      "name": "avg_order_value",
                      "expression": "`avg_order_value`"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "order_month",
                  "displayName": "Order Month",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fields": [
                    {
                      "fieldName": "total_sales",
                      "displayName": "Total Sales"
                    },
                    {
                      "fieldName": "order_count",
                      "displayName": "Order Count"
                    }
                  ],
                  "scale": {
                    "type": "quantitative"
                  }
                },
                "color": {
                  "fieldName": "product_category",
                  "displayName": "Product Category",
                  "scale": {
                    "type": "categorical"
                  }
                }
              },
              "frame": {
                "title": "Sales Trend by Product Category",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": [
        {
          "fieldName": "order_date",
          "displayName": "Order Date",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false
        },
        {
          "fieldName": "product_category",
          "displayName": "Product Category",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": true
        }
      ]
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}