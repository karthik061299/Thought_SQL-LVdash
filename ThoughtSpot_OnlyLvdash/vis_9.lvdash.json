{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention Dataset",
      "queryLines": [
        "WITH base_data AS (\n  SELECT\n    DATE_TRUNC('MONTH', first_device_event_date_time) as first_month,\n    months_since_first_use,\n    session_type,\n    SUM(active_devices) as active_devices\n  FROM\n    HPX_Retention_CSS_Analytics\n  WHERE\n    first_device_event_date_time >= '2023-05-01'\n  GROUP BY\n    DATE_TRUNC('MONTH', first_device_event_date_time),\n    months_since_first_use,\n    session_type\n)\n\nSELECT\n  first_month,\n  months_since_first_use,\n  session_type,\n  active_devices,\n  active_devices / SUM(active_devices) OVER (PARTITION BY first_month) as devices_returning_pct\nFROM\n  base_data\nORDER BY\n  first_month,\n  months_since_first_use"
      ]
    }
  ],
  "pages": [
    {
      "name": "hpx_retention_page",
      "displayName": "0.5 HPX Retention",
      "layout": [
        {
          "widget": {
            "name": "hpx_retention_pivot",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_retention_dataset",
                  "fields": [
                    { "name": "first_month", "expression": "`first_month`" },
                    { "name": "months_since_first_use", "expression": "`months_since_first_use`" },
                    { "name": "session_type", "expression": "`session_type`" },
                    { "name": "active_devices", "expression": "`active_devices`" },
                    { "name": "devices_returning_pct", "expression": "`devices_returning_pct`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pivot_table",
              "encodings": {
                "rows": [
                  {
                    "fieldName": "first_month",
                    "displayName": "Month(First Device Event Date Time)",
                    "scale": { "type": "temporal" }
                  }
                ],
                "columns": [
                  {
                    "fieldName": "months_since_first_use",
                    "displayName": "Months Since First Use",
                    "scale": { "type": "categorical" }
                  }
                ],
                "values": [
                  {
                    "fieldName": "devices_returning_pct",
                    "displayName": "% Devices Returning",
                    "scale": { "type": "quantitative" },
                    "format": { "type": "percent", "decimals": 1 }
                  }
                ],
                "color": {
                  "fieldName": "months_since_first_use",
                  "displayName": "Months Since First Use",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "Device Retention by Cohort",
                "showTitle": true
              },
              "options": {
                "heatmap": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}