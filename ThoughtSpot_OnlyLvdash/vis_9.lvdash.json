{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention Dataset",
      "queryLines": [
        "SELECT\n  DATE_TRUNC('MONTH', first_device_event_time) as first_device_event_month,\n  months_since_first_use,\n  COUNT(DISTINCT device_id) as active_devices,\n  COUNT(DISTINCT device_id) / FIRST_VALUE(COUNT(DISTINCT device_id)) OVER (PARTITION BY DATE_TRUNC('MONTH', first_device_event_time) ORDER BY months_since_first_use) as pct_devices_returning\nFROM HPX_Retention_CSS_Analytics\nWHERE\n  first_device_event_time >= '2023-05-01'\nGROUP BY DATE_TRUNC('MONTH', first_device_event_time), months_since_first_use\nORDER BY first_device_event_month, months_since_first_use"
      ]
    }
  ],
  "pages": [
    {
      "name": "hpx_retention_page",
      "displayName": "0.5 HPX Retention",
      "layout": [
        {
          "widget": {
            "name": "hpx_retention_pivot",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_retention_dataset",
                  "fields": [
                    { "name": "first_device_event_month", "expression": "`first_device_event_month`" },
                    { "name": "months_since_first_use", "expression": "`months_since_first_use`" },
                    { "name": "active_devices", "expression": "`active_devices`" },
                    { "name": "pct_devices_returning", "expression": "`pct_devices_returning`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pivot",
              "encodings": {
                "rows": [
                  {
                    "fieldName": "first_device_event_month",
                    "displayName": "First Device Event Month",
                    "scale": { "type": "temporal" }
                  }
                ],
                "columns": [
                  {
                    "fieldName": "months_since_first_use",
                    "displayName": "Months Since First Use",
                    "scale": { "type": "quantitative" }
                  }
                ],
                "values": [
                  {
                    "fieldName": "pct_devices_returning",
                    "displayName": "% Devices Returning",
                    "scale": { "type": "quantitative" },
                    "format": { "type": "percentage", "precision": 1 }
                  }
                ],
                "color": {
                  "fieldName": "pct_devices_returning",
                  "displayName": "% Devices Returning",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Device Retention by Cohort",
                "showTitle": true
              },
              "options": {
                "showRowTotals": false,
                "showColumnTotals": false,
                "showRowGrandTotals": false,
                "showColumnGrandTotals": false,
                "heatmap": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}