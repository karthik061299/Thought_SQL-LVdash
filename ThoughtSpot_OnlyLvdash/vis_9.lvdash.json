{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('MONTH', first_device_event_date_time) as monthly_first_device_event_date, months_since_first_use, session_type, weeks_since_first_use, SUM(active_devices) as active_devices, SUM(active_devices) / SUM(SUM(active_devices)) OVER (PARTITION BY DATE_TRUNC('MONTH', first_device_event_date_time)) as devices_returning_pct FROM HPX_Retention_CSS_Analytics WHERE first_device_event_date_time >= '2023-05-01' GROUP BY DATE_TRUNC('MONTH', first_device_event_date_time), months_since_first_use, session_type, weeks_since_first_use ORDER BY monthly_first_device_event_date, months_since_first_use"
      ]
    }
  ],
  "pages": [
    {
      "name": "hpx_retention_page",
      "displayName": "0.5 HPX Retention",
      "layout": [
        {
          "widget": {
            "name": "hpx_retention_pivot_table",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_retention_dataset",
                  "fields": [
                    { "name": "monthly_first_device_event_date", "expression": "`monthly_first_device_event_date`" },
                    { "name": "months_since_first_use", "expression": "`months_since_first_use`" },
                    { "name": "session_type", "expression": "`session_type`" },
                    { "name": "weeks_since_first_use", "expression": "`weeks_since_first_use`" },
                    { "name": "active_devices", "expression": "`active_devices`" },
                    { "name": "devices_returning_pct", "expression": "`devices_returning_pct`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "table",
              "encodings": {
                "rows": [
                  {
                    "fieldName": "monthly_first_device_event_date",
                    "displayName": "Month(First Device Event Date Time)",
                    "scale": { "type": "temporal" }
                  }
                ],
                "columns": [
                  {
                    "fieldName": "months_since_first_use",
                    "displayName": "Months Since First Use",
                    "scale": { "type": "categorical" }
                  }
                ],
                "values": [
                  {
                    "fieldName": "devices_returning_pct",
                    "displayName": "% Devices Returning",
                    "scale": { "type": "quantitative" }
                  }
                ]
              },
              "frame": {
                "title": "0.5 HPX Retention",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}