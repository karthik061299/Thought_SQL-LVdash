{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention Dataset",
      "queryLines": [
        "SELECT\n  DATE_TRUNC('MONTH', first_device_event_date_time) as monthly_first_device_event_date,\n  months_since_first_use,\n  session_type,\n  COUNT(DISTINCT device_id) as active_devices,\n  COUNT(DISTINCT device_id) / FIRST_VALUE(COUNT(DISTINCT device_id)) OVER (\n    PARTITION BY DATE_TRUNC('MONTH', first_device_event_date_time)\n    ORDER BY months_since_first_use\n    ROWS BETWEEN UNBOUNDED PRECEDING AND UNBOUNDED FOLLOWING\n  ) as pct_devices_returning\nFROM HPX_Retention_CSS_Analytics\nWHERE\n  first_device_event_date_time >= '2023-05-01'\nGROUP BY\n  DATE_TRUNC('MONTH', first_device_event_date_time),\n  months_since_first_use,\n  session_type\nORDER BY\n  monthly_first_device_event_date,\n  months_since_first_use"
      ]
    }
  ],
  "pages": [
    {
      "name": "hpx_retention_page",
      "displayName": "0.5 HPX Retention",
      "layout": [
        {
          "widget": {
            "name": "hpx_retention_pivot",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_retention_dataset",
                  "fields": [
                    { "name": "monthly_first_device_event_date", "expression": "`monthly_first_device_event_date`" },
                    { "name": "months_since_first_use", "expression": "`months_since_first_use`" },
                    { "name": "session_type", "expression": "`session_type`" },
                    { "name": "active_devices", "expression": "`active_devices`" },
                    { "name": "pct_devices_returning", "expression": "`pct_devices_returning`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pivotTable",
              "encodings": {
                "rows": [
                  {
                    "fieldName": "monthly_first_device_event_date",
                    "displayName": "Monthly First Device Event Date",
                    "scale": { "type": "temporal" }
                  }
                ],
                "columns": [
                  {
                    "fieldName": "months_since_first_use",
                    "displayName": "Months Since First Use",
                    "scale": { "type": "quantitative" }
                  }
                ],
                "values": [
                  {
                    "fieldName": "pct_devices_returning",
                    "displayName": "% Devices Returning",
                    "scale": { "type": "quantitative" },
                    "format": { "type": "percent", "precision": 1 }
                  }
                ],
                "color": {
                  "fieldName": "pct_devices_returning",
                  "displayName": "% Devices Returning",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "0.5 HPX Retention",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}