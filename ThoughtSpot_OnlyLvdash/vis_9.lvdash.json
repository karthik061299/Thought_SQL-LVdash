{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('MONTH', first_device_event_date_time) as monthly_first_device_event, active_devices, (active_devices / SUM(active_devices) OVER (PARTITION BY DATE_TRUNC('MONTH', first_device_event_date_time))) * 100 as pct_devices_returning, months_since_first_use, session_type, weeks_since_first_use FROM hpx_retention_css_analytics WHERE first_device_event_date_time >= '2023-05-01' ORDER BY monthly_first_device_event, months_since_first_use"
      ]
    }
  ],
  "pages": [
