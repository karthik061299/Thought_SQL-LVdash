{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('MONTH', first_device_event_time) as month_first_device_event_time, months_since_first_use, COUNT(DISTINCT device_id) as active_devices, COUNT(DISTINCT device_id) / SUM(COUNT(DISTINCT device_id)) OVER (PARTITION BY DATE_TRUNC('MONTH', first_device_event_time)) as pct_devices_returning FROM hpx_retention_css_analytics WHERE first_device_event_time >= '2023-05-01' GROUP BY DATE_TRUNC('MONTH', first_device_event_time), months_since_first_use ORDER BY month_first_device_event_time, months_since_first_use"
      ]
    }
  ],
  "pages": [
    {
      "name": "hpx_retention_page",
      "displayName": "0.5 HPX Retention",
      "layout": [
        {
          "widget": {
            "name": "hpx_retention_pivot_table",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_retention_dataset",
                  "fields": [
                    { "name": "month_first_device_event_time", "expression": "`month_first_device_event_time`" },
                    { "name": "months_since_first_use", "expression": "`months_since_first_use`" },
                    { "name": "active_devices", "expression": "`active_devices`" },
                    { "name": "pct_devices_returning", "expression": "`pct_devices_returning`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pivotTable",
              "encodings": {
                "rows": [
                  {
                    "fieldName": "month_first_device_event_time",
                    "displayName": "Month First Device Event Time"
                  }
                ],
                "columns": [
                  {
                    "fieldName": "months_since_first_use",
                    "displayName": "Months Since First Use"
                  }
                ],
                "values": [
                  {
                    "fieldName": "pct_devices_returning",
                    "displayName": "% Devices Returning",
                    "format": "PERCENTAGE"
                  }
                ]
              },
              "frame": {
                "title": "0.5 HPX Retention",
                "showTitle": true
              },
              "options": {
                "heatmap": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}