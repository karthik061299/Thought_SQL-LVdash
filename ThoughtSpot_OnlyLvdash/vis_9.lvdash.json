{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('MONTH', first_device_event_date_time) as month_first_device_event_date_time, months_since_first_use, session_type, weeks_since_first_use, SUM(active_devices) as active_devices, AVG(pct_devices_returning) as pct_devices_returning FROM hpx_retention_css_analytics WHERE first_device_event_date_time >= '2023-05-01' GROUP BY DATE_TRUNC('MONTH', first_device_event_date_time), months_since_first_use, session_type, weeks_since_first_use ORDER BY month_first_device_event_date_time, months_since_first_use"
      ]
    }
  ],
