{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention Analytics Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('MONTH', first_device_event_date_time) as month_first_device_event_date_time, months_since_first_use, session_type, weeks_since_first_use, SUM(active_devices) as active_devices, SUM(active_devices) / SUM(SUM(active_devices)) OVER (PARTITION BY DATE_TRUNC('MONTH', first_device_event_date_time)) * 100 as devices_returning_percentage FROM team_css_analytics_prod.hpx_analytics.hpx_retention_good_metric_view WHERE first_device_event_date_time >= '2023-05-01' GROUP BY DATE_TRUNC('MONTH', first_device_event_date_time), months_since_first_use, session_type, weeks_since_first_use ORDER BY month_first_device_event_date_time, months_since_first_use"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "HPX Retention Cohort Analysis",
      "layout": [
        {
          "widget": {
            "name": "hpx_retention_pivot_table",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_retention_dataset",
                  "fields": [
                    { "name": "month_first_device_event_date_time", "expression": "`month_first_device_event_date_time`" },
                    { "name": "months_since_first_use", "expression": "`months_since_first_use`" },
                    { "name": "session_type", "expression": "`session_type`" },
                    { "name": "weeks_since_first_use", "expression": "`weeks_since_first_use`" },
                    { "name": "active_devices", "expression": "`active_devices`" },
                    { "name": "devices_returning_percentage", "expression": "`devices_returning_percentage`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "table",
              "encodings": {
                "rows": [
                  {
                    "fieldName": "month_first_device_event_date_time",
                    "displayName": "Month (First Device Event Date Time)",
                    "scale": { "type": "temporal" }
                  }
                ],
                "columns": [
                  {
                    "fieldName": "months_since_first_use",
                    "displayName": "Months Since First Use",
                    "scale": { "type": "categorical" }
                  }
                ],
                "values": [
                  {
                    "fieldName": "devices_returning_percentage",
                    "displayName": "% Devices Returning",
                    "scale": { "type": "quantitative" }
                  },
                  {
                    "fieldName": "active_devices",
                    "displayName": "Active Devices",
                    "scale": { "type": "quantitative" }
                  }
                ],
                "filters": [
                  {
                    "fieldName": "session_type",
                    "displayName": "Session Type",
                    "scale": { "type": "categorical" }
                  },
                  {
                    "fieldName": "weeks_since_first_use",
                    "displayName": "Weeks Since First Use",
                    "scale": { "type": "categorical" }
                  }
                ]
              },
              "frame": {
                "title": "HPX Retention Cohort Analysis - % Devices Returning",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}