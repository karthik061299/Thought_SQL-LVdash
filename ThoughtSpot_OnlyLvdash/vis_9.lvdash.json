{
  "datasets": [
    {
      "name": "hpx_retention_dataset",
      "displayName": "HPX Retention Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('MONTH', first_device_event_date_time) as first_month, months_since_first_use, session_type, SUM(active_devices) as active_devices, SUM(active_devices) / SUM(SUM(active_devices)) OVER (PARTITION BY DATE_TRUNC('MONTH', first_device_event_date_time)) as pct_devices_returning FROM HPX_Retention_CSS_Analytics WHERE first_device_event_date_time >= '2023-05-01' GROUP BY DATE_TRUNC('MONTH', first_device_event_date_time), months_since_first_use, session_type ORDER BY first_month, months_since_first_use"
      ]
    }
  ],
  "pages": [
    {
      "name": "hpx_retention_page",
      "displayName": "0.5 HPX Retention",
      "layout": [
        {
          "widget": {
            "name": "hpx_retention_pivot",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_retention_dataset",
                  "fields": [
                    { "name": "first_month", "expression": "`first_month`" },
                    { "name": "months_since_first_use", "expression": "`months_since_first_use`" },
                    { "name": "session_type", "expression": "`session_type`" },
                    { "name": "active_devices", "expression": "`active_devices`" },
                    { "name": "pct_devices_returning", "expression": "`pct_devices_returning`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pivotTable",
              "encodings": {
                "rows": [
                  {
                    "fieldName": "first_month",
                    "displayName": "First Month"
                  }
                ],
                "columns": [
                  {
                    "fieldName": "months_since_first_use",
                    "displayName": "Months Since First Use"
                  }
                ],
                "values": [
                  {
                    "fieldName": "pct_devices_returning",
                    "displayName": "% Devices Returning",
                    "format": "PERCENTAGE"
                  }
                ],
                "color": {
                  "fieldName": "pct_devices_returning",
                  "displayName": "% Devices Returning",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Device Retention by Cohort",
                "showTitle": true
              },
              "options": {
                "heatmap": true,
                "showRowTotals": false,
                "showColumnTotals": false
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}