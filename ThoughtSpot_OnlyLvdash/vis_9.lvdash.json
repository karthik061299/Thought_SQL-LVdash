{
  "datasets": [
    {
      "name": "ds_hpx_retention",
      "displayName": "HPX Retention Dataset",
      "asset_name": "hive_metastore.default.mv_hpx_retention"
    }
  ],
  "pages": [
    {
      "name": "page_retention",
      "displayName": "Retention",
      "layout": [
        {
          "widget": {
            "name": "pivot_hpx_retention",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "ds_hpx_retention",
                  "fields": [
                    {
                      "name": "monthly_first_device_event_date_time",
                      "expression": "DATE_TRUNC('MONTH', `First Device Event Date Time`)"
                    },
                    {
                      "name": "months_since_first_use",
                      "expression": "`Months Since First Use`"
                    },
                    {
                      "name": "session_type",
                      "expression": "`Session Type`"
                    },
                    {
                      "name": "weeks_since_first_use",
                      "expression": "`Weeks Since First Use`"
                    },
                    {
                      "name": "active_devices",
                      "expression": "MEASURE(`Active Devices`)"
                    },
                    {
                      "name": "pct_devices_returning",
                      "expression": "MEASURE(`Active Devices`) / SUM(MEASURE(`Active Devices`)) OVER (PARTITION BY DATE_TRUNC('MONTH', `First Device Event Date Time`))"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pivot",
              "encodings": {
                "rows": [
                  {
                    "fieldName": "monthly_first_device_event_date_time",
                    "displayName": "Month(First Device Event Date Time)",
                    "scale": {
                      "type": "temporal"
                    }
                  }
                ],
                "columns": [
                  {
                    "fieldName": "months_since_first_use",
                    "displayName": "Months Since First Use",
                    "scale": {
                      "type": "categorical"
                    }
                  }
                ],
                "values": [
                  {
                    "fieldName": "pct_devices_returning",
                    "displayName": "% Devices Returning",
                    "scale": {
                      "type": "quantitative"
                    }
                  },
                  {
                    "fieldName": "active_devices",
                    "displayName": "Active Devices",
                    "scale": {
                      "type": "quantitative"
                    }
                  }
                ],
                "color": [
                  {
                    "fieldName": "session_type",
                    "displayName": "Session Type",
                    "scale": {
                      "type": "categorical"
                    }
                  }
                ]
              },
              "frame": {
                "title": "0.5 HPX Retention",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}