{
  "datasets": [
    {
      "name": "home_tile_secondary_ctr_dataset",
      "displayName": "Home Tile Secondary CTR Dataset",
      "queryLines": [
        "SELECT SUM(unique_interstitial_secondary_button_clicks) / SUM(unique_interstitial_views) as secondary_ctr FROM hpx_events_model_homepage_summary"
      ]
    }
  ],
  "pages": [
    {
      "name": "home_tile_secondary_ctr_page",
      "displayName": "Home Tile Secondary CTR",
      "layout": [
        {
          "widget": {
            "name": "home_tile_secondary_ctr_kpi",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "home_tile_secondary_ctr_dataset",
                  "fields": [
                    { "name": "secondary_ctr", "expression": "`secondary_ctr`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "secondary_ctr",
                  "displayName": "Secondary Button CTR",
                  "format": {
                    "type": "percent",
                    "decimalPlaces": 1
                  },
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Home Tile Secondary Button CTR",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 6, "height": 4 }
        },
        {
          "widget": {
            "name": "home_tile_secondary_ctr_table",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "home_tile_secondary_ctr_dataset",
                  "fields": [
                    { "name": "home_tile", "expression": "`home_tile`" },
                    { "name": "total_unique_interstitial_views", "expression": "`total_unique_interstitial_views`" },
                    { "name": "total_unique_interstitial_secondary_button_clicks", "expression": "`total_unique_interstitial_secondary_button_clicks`" },
                    { "name": "secondary_ctr", "expression": "`secondary_ctr`" }
                  ],
                  "disaggregated": false,
                  "queryLines": [
                    "SELECT home_tile, SUM(unique_interstitial_views) as total_unique_interstitial_views, SUM(unique_interstitial_secondary_button_clicks) as total_unique_interstitial_secondary_button_clicks, SUM(unique_interstitial_secondary_button_clicks) / SUM(unique_interstitial_views) as secondary_ctr FROM hpx_events_model_homepage_summary GROUP BY home_tile ORDER BY total_unique_interstitial_views DESC"
                  ]
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "table",
              "columns": [
                {
                  "fieldName": "home_tile",
                  "displayName": "Home Tile"
                },
                {
                  "fieldName": "total_unique_interstitial_views",
                  "displayName": "Unique Interstitial Views",
                  "format": {
                    "type": "number",
                    "decimalPlaces": 0
                  }
                },
                {
                  "fieldName": "total_unique_interstitial_secondary_button_clicks",
                  "displayName": "Unique Interstitial Secondary Button Clicks",
                  "format": {
                    "type": "number",
                    "decimalPlaces": 0
                  }
                },
                {
                  "fieldName": "secondary_ctr",
                  "displayName": "Secondary Button CTR",
                  "format": {
                    "type": "percent",
                    "decimalPlaces": 1
                  }
                }
              ],
              "frame": {
                "title": "Home Tile Secondary Button CTR by Tile",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 4, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}