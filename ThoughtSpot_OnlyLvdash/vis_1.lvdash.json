{
  "datasets": [
    {
      "name": "active_devices_dataset",
      "displayName": "Active Devices Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', event_date_time) as daily_event_date, COUNT(DISTINCT device_id) as active_devices FROM hpx_events_model GROUP BY DATE_TRUNC('DAY', event_date_time) ORDER BY daily_event_date"
      ]
    }
  ],
  "pages": [
    {
      "name": "active_devices_page",
      "displayName": "Active Devices",
      "layout": [
        {
          "widget": {
            "name": "active_devices_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "active_devices_dataset",
                  "fields": [
                    { "name": "daily_event_date", "expression": "`daily_event_date`" },
                    { "name": "active_devices", "expression": "`active_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "active_devices",
                  "displayName": "Active Devices",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Active Devices",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}