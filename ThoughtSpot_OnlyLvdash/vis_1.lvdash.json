{
  "datasets": [
    {
      "name": "hpx_events_model_dataset",
      "displayName": "HPX Events Model Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', event_date_time) as daily_event_date, SUM(active_devices_1) as total_active_devices FROM team_css_analytics_prod.hpx_analytics.hpx_events_model_iide_metric_view WHERE event_date_time >= '2023-05-01' GROUP BY DATE_TRUNC('DAY', event_date_time) ORDER BY daily_event_date"
      ]
    }
  ],
  "pages": [
    {
      "name": "executive_summary",
      "displayName": "Executive Summary",
      "layout": [
        {
          "widget": {
            "name": "active_devices_line_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_model_dataset",
                  "fields": [
                    { "name": "daily_event_date", "expression": "`daily_event_date`" },
                    { "name": "total_active_devices", "expression": "`total_active_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "total_active_devices",
                  "displayName": "Active Devices",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Active Devices Over Time",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 6, "height": 4 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": [
        {
          "fieldName": "event_date_time",
          "displayName": "Event Date Time",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false
        },
        {
          "fieldName": "geo_country_code",
          "displayName": "Top 10 Country",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false
        },
        {
          "fieldName": "app_version",
          "displayName": "App Version",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false
        },
        {
          "fieldName": "pc_type",
          "displayName": "PC Type",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false
        }
      ]
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}