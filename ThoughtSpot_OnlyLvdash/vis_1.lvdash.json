{
  "version": "1.0",
  "metadata": {
    "name": "Active Devices",
    "description": "HPX Events Model with IIDE Table",
    "created_by": "ThoughtSpot Migration Agent",
    "created_at": "2024-01-01T00:00:00Z"
  },
  "datasets": [
    {
      "name": "paas_tracking_card_metrics",
      "displayName": "PaaS Tracking Card Metrics",
      "query": "SELECT DATE_TRUNC('day', session_start_date_time) AS day_event_date_time, COUNT(DISTINCT aip_device_uuid) AS active_devices FROM team_css_analytics_prod.hpx_analytics.paas_tracking_card_metrics GROUP BY DATE_TRUNC('day', session_start_date_time) ORDER BY day_event_date_time"
    }
  ],
  "pages": [
    {
      "name": "Page 1",
      "displayName": "Active Devices Analysis",
      "layout": {
        "position": {
          "x": 0,
          "y": 0,
          "width": 6,
          "height": 4
        }
      },
      "spec": {
        "version": "3.0",
        "encodings": {
          "x": {
            "fieldName": "day_event_date_time",
            "displayName": "Daily Event Date",
            "scale": {
              "type": "temporal"
            }
          },
          "y": {
            "fieldName": "active_devices",
            "displayName": "Active Devices",
            "scale": {
              "type": "quantitative"
            }
          }
        },
        "mark": {
          "type": "line",
          "tooltip": true
        },
        "frame": {
          "showTitle": true,
          "title": "Active Devices Over Time"
        }
      },
      "datasets": [
        {
          "name": "paas_tracking_card_metrics",
          "displayName": "PaaS Tracking Card Metrics"
        }
      ]
    }
  ]
}