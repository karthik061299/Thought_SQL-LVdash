{
  "datasets": [
    {
      "name": "active_devices_by_appversion_dataset",
      "displayName": "Active Devices by Appversion Dataset",
      "queryLines": [
        "WITH app_version_devices AS (\n  SELECT\n    app_version,\n    COUNT(DISTINCT device_id) as active_devices\n  FROM hpx_events_model\n  WHERE\n    event_date_time >= '2023-05-01'\n    AND app_version NOT IN ('not a store release', 'other non-releases')\n    AND os_version IS NOT NULL\n  GROUP BY app_version\n),\ntotal_devices AS (\n  SELECT COUNT(DISTINCT device_id) as total_active_devices\n  FROM hpx_events_model\n  WHERE\n    event_date_time >= '2023-05-01'\n    AND app_version NOT IN ('not a store release', 'other non-releases')\n    AND os_version IS NOT NULL\n)\nSELECT\n  avd.app_version,\n  avd.active_devices / td.total_active_devices as pct_active_devices_by_appversion\nFROM app_version_devices avd, total_devices td\nORDER BY pct_active_devices_by_appversion DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "active_devices_by_appversion_page",
      "displayName": "% Active Devices by Appversion",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_appversion_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "active_devices_by_appversion_dataset",
                  "fields": [
                    { "name": "app_version", "expression": "`app_version`" },
                    { "name": "pct_active_devices_by_appversion", "expression": "`pct_active_devices_by_appversion`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "app_version",
                  "displayName": "App Version",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "pct_active_devices_by_appversion",
                  "displayName": "% Active Devices By Appversion",
                  "scale": { "type": "quantitative" },
                  "format": { "type": "percentage", "precision": 1 }
                }
              },
              "frame": {
                "title": "% Active Devices by Appversion",
                "showTitle": true
              },
              "options": {
                "showDataLabels": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}