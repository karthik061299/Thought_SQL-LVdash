{
  "datasets": [
    {
      "name": "active_devices_by_appversion_dataset",
      "displayName": "Active Devices by App Version Dataset",
      "queryLines": [
        "SELECT app_version, SUM(active_devices) as total_active_devices, SUM(active_devices) / SUM(SUM(active_devices)) OVER () as pct_active_devices FROM hpx_events_model WHERE event_date_time >= '2023-05-01' AND app_version NOT IN ('not a store release', 'other non-releases') AND os_version IS NOT NULL GROUP BY app_version ORDER BY pct_active_devices DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "active_devices_by_appversion_page",
      "displayName": "% Active Devices by Appversion",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_appversion_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "active_devices_by_appversion_dataset",
                  "fields": [
                    { "name": "app_version", "expression": "`app_version`" },
                    { "name": "pct_active_devices", "expression": "`pct_active_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "app_version",
                  "displayName": "App Version",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "pct_active_devices",
                  "displayName": "% Active Devices By Appversion",
                  "format": {
                    "type": "percent",
                    "decimalPlaces": 1
                  },
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "% Active Devices by App Version",
                "showTitle": true
              },
              "options": {
                "showDataLabels": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}