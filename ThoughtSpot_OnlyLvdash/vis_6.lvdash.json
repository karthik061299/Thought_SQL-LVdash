{
  "version": "1.0",
  "displayName": "Top Unique Screen Views (excl. Home/Welcome)",
  "description": "HPX Events Model with IIDE Table",
  "datasets": [
    {
      "name": "hpx_events_model_dataset",
      "displayName": "HPX Events Model Dataset",
      "query": "SELECT screen, unique_screen_views_1 FROM team_css_analytics_prod.hpx_analytics.hpx_events_model_iide_metric_view WHERE ui_action = 'screendisplayed' AND screen NOT IN ('home', 'support', 'welcome', 'welcome-core', 'welcomeContainer') AND screen IS NOT NULL AND event_date_time >= '2023-05-01' ORDER BY unique_screen_views_1 DESC LIMIT 10"
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "Top Unique Screen Views",
      "layout": [
        {
          "widget": {
            "name": "viz_6_widget",
            "textbox_spec": null,
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "dataset_name": "hpx_events_model_dataset",
                  "disaggregated": false,
                  "fields": [
                    {
                      "name": "screen",
                      "expression": "screen"
                    },
                    {
                      "name": "unique_screen_views_1",
                      "expression": "SUM(unique_screen_views_1)"
                    }
                  ],
                  "filters": [
                    {
                      "column": "ui_action",
                      "operator": "=",
                      "values": ["screendisplayed"]
                    },
                    {
                      "column": "screen",
                      "operator": "NOT IN",
                      "values": ["home", "support", "welcome", "welcome-core", "welcomeContainer"]
                    },
                    {
                      "column": "screen",
                      "operator": "IS NOT NULL",
                      "values": []
                    },
                    {
                      "column": "event_date_time",
                      "operator": ">=",
                      "values": ["2023-05-01"]
                    }
                  ],
                  "order_by": [
                    {
                      "field": "unique_screen_views_1",
                      "direction": "DESC"
                    }
                  ],
                  "limit": 10
                }
              }
            ],
            "spec": {
              "version": "3.0",
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "screen",
                  "displayName": "Screen",
                  "scale": {
                    "type": "categorical"
                  }
                },
                "y": {
                  "fieldName": "unique_screen_views_1",
                  "displayName": "Unique Screen Views",
                  "scale": {
                    "type": "quantitative"
                  }
                }
              },
              "mark": "bar",
              "color": {
                "fieldName": "unique_screen_views_1",
                "displayName": "Unique Screen Views",
                "scale": {
                  "type": "quantitative",
                  "range": ["#0096D3"]
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 6,
            "height": 4
          }
        }
      ]
    }
  ]
}