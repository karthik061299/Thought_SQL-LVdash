{
  "datasets": [
    {
      "name": "top_screen_views_summary_dataset",
      "displayName": "Top Unique Screen Views Summary Dataset",
      "queryLines": [
        "SELECT screen, SUM(unique_screen_views) as total_unique_screen_views FROM hpx_events_model WHERE event_date_time >= '2023-05-01' AND ui_action = 'screendisplayed' AND screen NOT IN ('home', 'welcome', 'welcomecontainer', 'welcome-core', 'support') GROUP BY screen ORDER BY SUM(unique_screen_views) DESC LIMIT 10"
      ]
    }
  ],
  "pages": [
    {
      "name": "top_screen_views_summary_page",
      "displayName": "Top Unique Screen Views (excl. Home/Welcome)",
      "layout": [
        {
          "widget": {
            "name": "top_screen_views_summary_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "top_screen_views_summary_dataset",
                  "fields": [
                    { "name": "screen", "expression": "`screen`" },
                    { "name": "total_unique_screen_views", "expression": "`total_unique_screen_views`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "y": {
                  "fieldName": "screen",
                  "displayName": "Screen",
                  "scale": { "type": "categorical" }
                },
                "x": {
                  "fieldName": "total_unique_screen_views",
                  "displayName": "Unique Screen Views",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Top Unique Screen Views (excl. Home/Welcome)",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}