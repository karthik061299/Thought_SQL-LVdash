{
  "version": "1.0",
  "metadata": {
    "name": "Top Unique Screen Views (excl. Home/Welcome) - Bar Chart",
    "description": "HPX Events Model with IIDE Table",
    "created_by": "ThoughtSpot Migration Agent",
    "created_at": "2024-01-01T00:00:00Z",
    "tags": ["thoughtspot-migration", "screen-views", "bar-chart"]
  },
  "datasets": [
    {
      "name": "hpx_events_model",
      "query": "SELECT screen, COUNT(DISTINCT session_id) AS unique_screen_views FROM team_css_analytics_prod.hpx_analytics.hpx_events_model WHERE event_date_time >= '2023-05-01' AND screen NOT IN ('home', 'welcome', 'welcomecontainer', 'welcome-core', 'support') AND ui_action = 'screendisplayed' AND screen IS NOT NULL GROUP BY screen ORDER BY unique_screen_views DESC LIMIT 10"
    }
  ],
  "widgets": [
    {
      "id": "viz_6_unique_screen_views_bar",
      "type": "bar_chart",
      "title": "Top Unique Screen Views (excl. Home/Welcome)",
      "description": "Top 10 screens by unique screen views",
      "position": {
        "x": 6,
        "y": 8,
        "width": 6,
        "height": 4
      },
      "query": {
        "dataset": "hpx_events_model",
        "fields": [
          {
            "name": "screen",
            "type": "dimension"
          },
          {
            "name": "unique_screen_views",
            "type": "measure"
          }
        ]
      },
      "encoding": {
        "x": {
          "fieldName": "screen",
          "displayName": "Screen",
          "scale": {
            "type": "categorical"
          }
        },
        "y": {
          "fieldName": "unique_screen_views",
          "displayName": "Unique Screen Views",
          "scale": {
            "type": "quantitative"
          }
        }
      },
      "visualization": {
        "type": "bar",
        "properties": {
          "orientation": "horizontal",
          "colors": ["#0096D3"]
        }
      }
    }
  ],
  "layout": {
    "type": "grid",
    "columns": 12,
    "rows": 8
  },
  "filters": [
    {
      "field": "event_date_time",
      "operator": ">=",
      "value": "2023-05-01"
    },
    {
      "field": "ui_action",
      "operator": "=",
      "value": "screendisplayed"
    }
  ],
  "parameters": []
}