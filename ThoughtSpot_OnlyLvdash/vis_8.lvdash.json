{
  "version": "1.0",
  "metadata": {
    "name": "Launch type Active Devices",
    "description": "HPX Events Model with IIDE Table - Stacked Column Chart showing Active Devices by Launch Type over Time",
    "created_by": "ThoughtSpot Migration Agent",
    "created_at": "2024-01-01T00:00:00Z",
    "tags": ["hpx_events", "active_devices", "launch_type", "stacked_column"]
  },
  "datasets": [
    {
      "name": "hpx_events_model_iide_dataset",
      "displayName": "HPX Events Model IIDE",
      "query": "SELECT DATE_TRUNC('DAY', event_date_time) as daily_event_date_time, launch_type, SUM(active_devices_1) as total_active_devices FROM team_css_analytics_prod.hpx_analytics.hpx_events_model_iide_metric_view WHERE launch_type IS NOT NULL AND launch_type != 'Null' GROUP BY DATE_TRUNC('DAY', event_date_time), launch_type ORDER BY daily_event_date_time, launch_type"
    }
  ],
  "pages": [
    {
      "name": "Launch Type Active Devices Page",
      "displayName": "Launch Type Active Devices",
      "layout": {
        "position": {
          "x": 0,
          "y": 0,
          "width": 12,
          "height": 8
        }
      },
      "widgets": [
        {
          "name": "launch_type_active_devices_chart",
          "type": "chart",
          "displayName": "Launch Type Active Devices",
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          },
          "spec": {
            "version": "3.0",
            "encodings": {
              "x": {
                "fieldName": "daily_event_date_time",
                "displayName": "Daily Event Date",
                "scale": {
                  "type": "temporal"
                }
              },
              "y": {
                "fieldName": "total_active_devices",
                "displayName": "Total % Event ID",
                "scale": {
                  "type": "quantitative"
                }
              },
              "color": {
                "fieldName": "launch_type",
                "displayName": "Launch Type",
                "scale": {
                  "type": "categorical"
                }
              }
            },
            "mark": {
              "type": "column",
              "stacked": true,
              "stackedAsPercent": true
            },
            "format": {
              "y": {
                "category": "PERCENTAGE",
                "percentageFormatConfig": {
                  "decimals": 0
                },
                "isCategoryEditable": false
              }
            },
            "colors": {
              "seriesColors": [
                {
                  "serieName": "automatic",
                  "color": "#BCBCBC"
                },
                {
                  "serieName": "manual",
                  "color": "#0096D3"
                }
              ]
            }
          },
          "queries": [
            {
              "name": "launch_type_active_devices_query",
              "query": {
                "datasetName": "hpx_events_model_iide_dataset",
                "fields": [
                  {
                    "name": "daily_event_date_time",
                    "expression": "daily_event_date_time"
                  },
                  {
                    "name": "launch_type",
                    "expression": "launch_type"
                  },
                  {
                    "name": "total_active_devices",
                    "expression": "total_active_devices"
                  }
                ],
                "filters": [
                  {
                    "column": "launch_type",
                    "operator": "IS_NOT_NULL"
                  },
                  {
                    "column": "launch_type",
                    "operator": "NOT_EQUALS",
                    "values": ["Null"]
                  }
                ]
              }
            }
          ]
        }
      ]
    }
  ]
}