{
  "datasets": [
    {
      "name": "launch_type_active_devices_dataset",
      "displayName": "HPX Events Model with IIDE Table",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', event_date_time) as daily_event_date, launch_type, SUM(active_devices) as active_devices FROM hpx_events_model WHERE launch_type IS NOT NULL AND launch_type IN ('Automatic', 'Manual') GROUP BY DATE_TRUNC('DAY', event_date_time), launch_type ORDER BY daily_event_date, launch_type"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "Launch type Active Devices",
      "layout": [
        {
          "widget": {
            "name": "launch_type_stacked_column",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "launch_type_active_devices_dataset",
                  "fields": [
                    { "name": "daily_event_date", "expression": "`daily_event_date`" },
                    { "name": "launch_type", "expression": "`launch_type`" },
                    { "name": "active_devices", "expression": "`active_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "active_devices",
                  "displayName": "Active Devices",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "launch_type",
                  "displayName": "Launch Type",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "Launch type Active Devices",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}