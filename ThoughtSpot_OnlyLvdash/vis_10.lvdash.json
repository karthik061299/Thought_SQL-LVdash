{
  "datasets": [
    {
      "name": "hpx_events_model_dataset",
      "displayName": "HPX Events Model Dataset",
      "queryLines": [
        "SELECT COUNT(DISTINCT sys_sn_hash) AS active_devices FROM team_css_analytics_prod.hpx_analytics.hpx_events_model_iide_metric_view WHERE app_version != 'not a store release' AND app_version IS NOT NULL AND event_date_time >= '2023-05-01' AND geo_country_code IS NOT NULL AND geo_country_code != 'null'"
      ]
    }
  ],
  "pages": [
    {
      "name": "executive_summary",
      "displayName": "Executive Summary",
      "layout": [
        {
          "widget": {
            "name": "total_consented_devices_counter",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_model_dataset",
                  "fields": [
                    {
                      "name": "active_devices",
                      "expression": "`active_devices`"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "active_devices",
                  "displayName": "Total Consented Devices"
                }
              },
              "frame": {
                "title": "Total Consented Devices",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 6,
            "y": 0,
            "width": 2,
            "height": 4
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}