{
  "datasets": [
    {
      "name": "total_consented_devices_dataset",
      "displayName": "Total Consented Devices Dataset",
      "queryLines": [
        "SELECT\n  COUNT(DISTINCT device_id) as active_devices\nFROM hpx_events_model\nWHERE\n  event_date_time >= '2023-05-01'\n  AND app_version NOT IN ('not a store release', NULL)\n  AND geo_country_code IS NOT NULL\n  AND pc_type IN ('consumer', 'enterprise', 'premium', 'rpos', 'sbso', 'unknown', 'workstation')"
      ]
    }
  ],
  "pages": [
    {
      "name": "total_consented_devices_page",
      "displayName": "Total Consented Devices",
      "layout": [
        {
          "widget": {
            "name": "total_consented_devices_counter",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "total_consented_devices_dataset",
                  "fields": [
                    { "name": "active_devices", "expression": "`active_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "active_devices",
                  "displayName": "Active Devices",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Total Consented Devices",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 6, "height": 4 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}