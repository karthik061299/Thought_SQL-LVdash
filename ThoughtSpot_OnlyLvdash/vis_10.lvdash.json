{
  "datasets": [
    {
      "name": "total_consented_devices_dataset",
      "displayName": "Total Consented Devices Dataset",
      "queryLines": [
        "SELECT SUM(active_devices) as total_active_devices FROM hpx_events_model WHERE app_version NOT IN ('not a store release', 'null') AND app_version IS NOT NULL AND event_date_time >= '2023-05-01' AND pc_type IN ('consumer', 'enterprise', 'premium', 'rpos', 'sbso', 'unknown', 'workstation') AND geo_country_code IN ('australia', 'brazil', 'canada', 'china', 'france', 'india', 'japan', 'mexico', 'other_countries', 'united kingdom', 'united states') AND geo_country_code IS NOT NULL"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_10",
      "displayName": "Total Consented Devices",
      "layout": [
        {
          "widget": {
            "name": "total_consented_devices_kpi",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "total_consented_devices_dataset",
                  "fields": [
                    { "name": "total_active_devices", "expression": "`total_active_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_active_devices",
                  "displayName": "Active Devices",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Total Consented Devices",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}