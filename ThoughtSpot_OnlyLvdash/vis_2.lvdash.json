  "datasets": [
    {
      "id": "hpx_events_model",
      "name": "HPX Events Model",
      "query": "SELECT DATE_TRUNC('day', event_date_time) AS daily_event_date, SUM(new_devices) AS new_devices, SUM(cumulative_new_devices) AS cumulative_new_devices FROM team_css_analytics_prod.hpx_analytics.hpx_events_model GROUP BY DATE_TRUNC('day', event_date_time) ORDER BY daily_event_date"
    }
  ],
