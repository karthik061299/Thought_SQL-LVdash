{
  "datasets": [
    {
      "name": "hpx_events_model_dataset",
      "displayName": "HPX Events Model Dataset",
      "queryLines": [
        "SELECT * FROM hive_metastore.default.mv_hpx_events_model"
      ]
    }
  ],
  "pages": [
    {
      "name": "retention_page",
      "displayName": "Retention",
      "layout": [
        {
          "widget": {
            "name": "hpx_new_version_adoption_line_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_model_dataset",
                  "fields": [
                    {
                      "name": "daily_event_date",
                      "expression": "DATE_TRUNC('DAY', `Event Date Time`)"
                    },
                    {
                      "name": "app_version",
                      "expression": "`App Version`"
                    },
                    {
                      "name": "active_devices",
                      "expression": "SUM(`Active Devices`)"
                    },
                    {
                      "name": "pct_active_devices",
                      "expression": "SUM(`Active Devices`) / SUM(SUM(`Active Devices`)) OVER (PARTITION BY DATE_TRUNC('DAY', `Event Date Time`))"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fieldName": "pct_active_devices",
                  "displayName": "% Active Devices",
                  "scale": {
                    "type": "quantitative"
                  }
                },
                "color": {
                  "fieldName": "app_version",
                  "displayName": "App Version",
                  "scale": {
                    "type": "categorical"
                  }
                }
              },
              "frame": {
                "title": "HPX New Version Adoption",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 5,
            "y": 0,
            "width": 7,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}