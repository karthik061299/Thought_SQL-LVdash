{
  "version": "1.0",
  "metadata": {
    "name": "HPX New Version Adoption",
    "description": "HPX Events Model with IIDE Table",
    "created_by": "ThoughtSpot Migration Agent",
    "created_at": "2024-01-01T00:00:00Z",
    "tags": ["hpx", "version_adoption", "line_chart"]
  },
  "datasets": [
    {
      "name": "hpx_events_model_dataset",
      "displayName": "HPX Events Model Dataset",
      "query": "SELECT DATE_TRUNC('DAY', event_date_time) AS day_event_date_time, app_version, COUNT(DISTINCT sys_sn_hash) AS active_devices, (COUNT(DISTINCT sys_sn_hash) / SUM(COUNT(DISTINCT sys_sn_hash)) OVER (PARTITION BY DATE_TRUNC('DAY', event_date_time))) * 100 AS pct_active_devices FROM team_css_analytics_prod.hpx_analytics.hpx_events_model_iide_metric_view GROUP BY DATE_TRUNC('DAY', event_date_time), app_version ORDER BY day_event_date_time, app_version"
    }
  ],
  "pages": [
    {
      "name": "HPX New Version Adoption Page",
      "displayName": "HPX New Version Adoption",
      "layout": {
        "position": {
          "x": 0,
          "y": 0,
          "width": 6,
          "height": 4
        }
      },
      "spec": {
        "version": "5.15.0",
        "data": {
          "name": "hpx_events_model_dataset"
        },
        "mark": {
          "type": "line",
          "point": true,
          "tooltip": true
        },
        "encoding": {
          "x": {
            "field": "day_event_date_time",
            "type": "temporal",
            "title": "Daily Event Date",
            "scale": {
              "type": "temporal"
            },
            "axis": {
              "title": "Daily Event Date",
              "format": "%Y-%m-%d"
            }
          },
          "y": {
            "field": "pct_active_devices",
            "type": "quantitative",
            "title": "% Active Devices",
            "scale": {
              "type": "quantitative"
            },
            "axis": {
              "title": "% Active Devices",
              "format": ".0%"
            }
          },
          "color": {
            "field": "app_version",
            "type": "nominal",
            "title": "App Version",
            "scale": {
              "type": "categorical",
              "scheme": "category20"
            },
            "legend": {
              "title": "App Version",
              "orient": "right"
            }
          }
        },
        "title": {
          "text": "HPX New Version Adoption",
          "subtitle": "HPX Events Model with IIDE Table",
          "anchor": "start"
        },
        "width": 600,
        "height": 400
      }
    }
  ],
