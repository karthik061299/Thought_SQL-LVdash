{
  "datasets": [
    {
      "name": "ds_hpx_events_model",
      "displayName": "HPX Events Model Dataset",
      "asset_name": "hive_metastore.default.mv_hpx_events_model",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', `Event Date Time`) as daily_event_date, `App Version`, MEASURE(`Active Devices`) as active_devices, (MEASURE(`Active Devices`) / SUM(MEASURE(`Active Devices`)) OVER (PARTITION BY DATE_TRUNC('DAY', `Event Date Time`))) * 100 as pct_active_devices FROM hive_metastore.default.mv_hpx_events_model GROUP BY DATE_TRUNC('DAY', `Event Date Time`), `App Version` ORDER BY daily_event_date, `App Version`"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_retention",
      "displayName": "Retention",
      "layout": [
        {
          "widget": {
            "name": "line_hpx_version_adoption",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "ds_hpx_events_model",
                  "fields": [
                    {
                      "name": "daily_event_date",
                      "expression": "`daily_event_date`"
                    },
                    {
                      "name": "App Version",
                      "expression": "`App Version`"
                    },
                    {
                      "name": "pct_active_devices",
                      "expression": "`pct_active_devices`"
                    },
                    {
                      "name": "active_devices",
                      "expression": "`active_devices`"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fieldName": "pct_active_devices",
                  "displayName": "% Active Devices",
                  "scale": {
                    "type": "quantitative"
                  }
                },
                "color": {
                  "fieldName": "App Version",
                  "displayName": "App Version",
                  "scale": {
                    "type": "categorical"
                  }
                }
              },
              "frame": {
                "title": "HPX New Version Adoption",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 5,
            "y": 0,
            "width": 7,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}