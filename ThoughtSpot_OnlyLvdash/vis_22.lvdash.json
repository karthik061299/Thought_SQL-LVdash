{
  "datasets": [
    {
      "name": "hpx_version_adoption_dataset",
      "displayName": "HPX Events Model with IIDE Table",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', event_date_time) as daily_event_date, app_version, SUM(active_devices) as active_devices, (SUM(active_devices) * 100.0 / SUM(SUM(active_devices)) OVER (PARTITION BY DATE_TRUNC('DAY', event_date_time))) as pct_active_devices FROM hpx_events_model GROUP BY DATE_TRUNC('DAY', event_date_time), app_version ORDER BY daily_event_date, app_version"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "HPX New Version Adoption",
      "layout": [
        {
          "widget": {
            "name": "version_adoption_line_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_version_adoption_dataset",
                  "fields": [
                    { "name": "daily_event_date", "expression": "`daily_event_date`" },
                    { "name": "app_version", "expression": "`app_version`" },
                    { "name": "pct_active_devices", "expression": "`pct_active_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "pct_active_devices",
                  "displayName": "% Active Devices",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "app_version",
                  "displayName": "App Version",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "HPX New Version Adoption",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}