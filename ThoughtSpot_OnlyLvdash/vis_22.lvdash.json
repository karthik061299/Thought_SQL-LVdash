{
  "datasets": [
    {
      "name": "hpx_events_model_dataset",
      "displayName": "HPX Events Model Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', event_date_time) as day_event_date_time, app_version, COUNT(DISTINCT sys_sn_hash) as active_devices, (COUNT(DISTINCT sys_sn_hash) / SUM(COUNT(DISTINCT sys_sn_hash)) OVER (PARTITION BY DATE_TRUNC('DAY', event_date_time))) * 100 as pct_active_devices FROM team_css_analytics_prod.hpx_analytics.hpx_events_model_iide_metric_view WHERE event_date_time >= '2023-05-01' GROUP BY DATE_TRUNC('DAY', event_date_time), app_version ORDER BY day_event_date_time, app_version"
      ]
    }
  ],
  "pages": [
    {
      "name": "retention_page",
      "displayName": "Retention",
      "layout": [
        {
          "widget": {
            "name": "hpx_new_version_adoption_line",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_model_dataset",
                  "fields": [
                    {
                      "name": "day_event_date_time",
                      "expression": "`day_event_date_time`"
                    },
                    {
                      "name": "app_version",
                      "expression": "`app_version`"
                    },
                    {
                      "name": "active_devices",
                      "expression": "`active_devices`"
                    },
                    {
                      "name": "pct_active_devices",
                      "expression": "`pct_active_devices`"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "day_event_date_time",
                  "displayName": "Daily Event Date",
                  "scale": {
                    "type": "temporal"
                  }
                },
                "y": {
                  "fieldName": "pct_active_devices",
                  "displayName": "% Active Devices",
                  "scale": {
                    "type": "quantitative"
                  }
                },
                "color": {
                  "fieldName": "app_version",
                  "displayName": "App Version",
                  "scale": {
                    "type": "categorical"
                  }
                }
              },
              "frame": {
                "title": "HPX New Version Adoption",
                "showTitle": true
              }
            }
          },
          "position": {
            "x": 5,
            "y": 0,
            "width": 7,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}