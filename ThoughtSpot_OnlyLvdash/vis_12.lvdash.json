{
  "datasets": [
    {
      "name": "hpx_curr_data_table_dataset",
      "displayName": "HPX CURR Data Table Dataset",
      "queryLines": [
        "SELECT\n  DATE_TRUNC('WEEK', week_active) as week_active,\n  SUM(previous_week_users_t14t20) as previous_week_users_t14t20,\n  SUM(last_week_users_t7t13) as last_week_users_t7t13,\n  SUM(this_week_users_t0t6) as this_week_users_t0t6,\n  AVG(curr) as curr\nFROM HPX_CURR\nWHERE\n  week_active >= '2023-05-01'\nGROUP BY DATE_TRUNC('WEEK', week_active)\nORDER BY week_active"
      ]
    }
  ],
  "pages": [
    {
      "name": "hpx_curr_data_table_page",
