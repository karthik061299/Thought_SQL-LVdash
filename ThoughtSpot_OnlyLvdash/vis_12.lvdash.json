{
  "datasets": [
    {
      "name": "hpx_curr_data_table_dataset",
      "displayName": "HPX CURR Data Table Dataset",
      "queryLines": [
        "SELECT\n  DATE_TRUNC('WEEK', week_active) as week_active,\n  AVG(curr) as avg_curr,\n  SUM(this_week_users_t0t6) as total_this_week_users,\n  SUM(last_week_users_t7t13) as total_last_week_users,\n  SUM(previous_week_users_t14t20) as total_previous_week_users\nFROM\n  HPX_CURR\nWHERE\n  week_active >= '2023-05-01'\nGROUP BY\n  DATE_TRUNC('WEEK', week_active)\nORDER BY\n  week_active"
      ]
    }
  ],
