{
  "datasets": [
    {
      "name": "hpx_curr_data_table_dataset",
      "displayName": "HPX CURR Data Table Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('WEEK', week_active) as weekly_active, SUM(previous_week_users_t14t20) as total_previous_week_users, SUM(last_week_users_t7t13) as total_last_week_users, SUM(this_week_users_t0t6) as total_this_week_users, AVG(curr) as avg_curr FROM HPX_CURR WHERE week_active >= '2023-05-01' GROUP BY DATE_TRUNC('WEEK', week_active) ORDER BY weekly_active"
      ]
    }
  ],
