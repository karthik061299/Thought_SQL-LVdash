{
  "name": "global_filter_001",
  "displayName": "Global Filters",
  "datasets": [
    {
      "name": "hpx_events_dataset",
      "displayName": "HPX Events Dataset",
      "queryLines": [
        "SELECT event_time, geo_market, geo_country, os_version, app_version, pc_type, pc_model_name, geo_country_code, event_date_time, home_tile FROM ex_schema.ex_hpx_events_table WHERE event_time >= '2023-05-01' AND geo_country_code IS NOT NULL AND app_version NOT IN ('Not a Store Release')"
      ]
    }
  ],
  "pages": [
    {
      "name": "global_filters_page",
      "displayName": "Global Filters",
      "layout": [
        {
          "widget": {
            "name": "event_time_filter",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_dataset",
                  "fields": [
                    {
                      "name": "event_time",
                      "expression": "`event_time`"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 2,
              "widgetType": "filter-date-picker",
              "encodings": {
                "fields": [
                  {
                    "fieldName": "event_time",
                    "displayName": "Event Time",
                    "queryName": "dashboards/hpx_dashboard/datasets/hpx_events_dataset_event_time"
                  }
                ]
              },
              "frame": {
                "showTitle": true,
                "title": "Event Time Filter"
              },
              "selection": {
                "defaultSelection": {
                  "values": {
                    "dataType": "DATETIME",
                    "values": [
                      {
                        "value": "2023-05-01T00:00:00.000"
                      }
                    ]
                  }
                }
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 3,
            "height": 2
          }
        },
        {
