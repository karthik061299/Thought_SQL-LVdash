{
  "datasets": [
    {
      "name": "hpx_events_model_iide_metric_view",
      "displayName": "HPX Events Model IIDE Metric View",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', event_date_time) AS daily_event_date, COUNT(DISTINCT sys_sn_hash) AS active_devices_1 FROM team_css_analytics_prod.hpx_analytics.hpx_events_model_iide_metric_view GROUP BY DATE_TRUNC('DAY', event_date_time) ORDER BY daily_event_date"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "Active Devices",
      "layout": [
        {
          "widget": {
            "name": "active_devices_line_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_model_iide_metric_view",
                  "fields": [
                    { "name": "daily_event_date", "expression": "`daily_event_date`" },
                    { "name": "active_devices_1", "expression": "`active_devices_1`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "active_devices_1",
                  "displayName": "Active Devices",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Active Devices by Day",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}
