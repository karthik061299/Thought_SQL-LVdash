{
  "datasets": [
    {
      "name": "active_devices_by_country_dataset",
      "displayName": "Active Devices by Country Dataset",
      "queryLines": [
        "SELECT geo_country_code, (SUM(active_devices) * 100.0 / (SELECT SUM(active_devices) FROM hpx_events_model WHERE app_version NOT IN ('not a store release', 'other non-releases') AND os_version IS NOT NULL AND event_date_time >= '2023-05-01')) as pct_active_devices_by_country, SUM(active_devices) as mau FROM hpx_events_model WHERE app_version NOT IN ('not a store release', 'other non-releases') AND os_version IS NOT NULL AND event_date_time >= '2023-05-01' GROUP BY geo_country_code ORDER BY mau DESC LIMIT 10"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_20",
      "displayName": "% Active Devices by Country (Top 10)",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_country_bar_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "active_devices_by_country_dataset",
                  "fields": [
                    { "name": "geo_country_code", "expression": "`geo_country_code`" },
                    { "name": "pct_active_devices_by_country", "expression": "`pct_active_devices_by_country`" },
                    { "name": "mau", "expression": "`mau`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "geo_country_code",
                  "displayName": "Country",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "pct_active_devices_by_country",
                  "displayName": "% Active Devices by Country",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "% Active Devices by Country (Top 10)",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}