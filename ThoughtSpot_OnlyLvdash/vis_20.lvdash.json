{
  "datasets": [
    {
      "name": "active_devices_by_country_dataset",
      "displayName": "Active Devices by Country Dataset",
      "queryLines": [
        "WITH country_devices AS (\n  SELECT\n    geo_country_code,\n    COUNT(DISTINCT device_id) as active_devices\n  FROM hpx_events_model\n  WHERE\n    event_date_time >= '2023-05-01'\n    AND app_version NOT IN ('not a store release', 'other non-releases')\n    AND os_version IS NOT NULL\n  GROUP BY geo_country_code\n),\ntotal_devices AS (\n  SELECT COUNT(DISTINCT device_id) as total_active_devices\n  FROM hpx_events_model\n  WHERE\n    event_date_time >= '2023-05-01'\n    AND app_version NOT IN ('not a store release', 'other non-releases')\n    AND os_version IS NOT NULL\n)\nSELECT\n  cd.geo_country_code,\n  cd.active_devices as mau,\n  cd.active_devices / td.total_active_devices as pct_active_devices_by_country,\n  RANK() OVER (ORDER BY cd.active_devices DESC) as active_devices_rank\nFROM country_devices cd, total_devices td\nWHERE active_devices_rank <= 10\nORDER BY active_devices DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "active_devices_by_country_page",
      "displayName": "% Active Devices by Country (Top 10)",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_country_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "active_devices_by_country_dataset",
                  "fields": [
                    { "name": "geo_country_code", "expression": "`geo_country_code`" },
                    { "name": "pct_active_devices_by_country", "expression": "`pct_active_devices_by_country`" },
                    { "name": "mau", "expression": "`mau`" },
                    { "name": "active_devices_rank", "expression": "`active_devices_rank`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "geo_country_code",
                  "displayName": "Country",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "pct_active_devices_by_country",
                  "displayName": "% Active Devices by Country",
                  "scale": { "type": "quantitative" },
                  "format": { "type": "percentage", "precision": 1 }
                },
                "tooltip": [
                  {
                    "fieldName": "geo_country_code",
                    "displayName": "Country"
                  },
                  {
                    "fieldName": "pct_active_devices_by_country",
                    "displayName": "% Active Devices by Country",
                    "format": { "type": "percentage", "precision": 1 }
                  },
                  {
                    "fieldName": "mau",
                    "displayName": "MAU",
                    "format": { "type": "number", "precision": 0 }
                  }
                ]
              },
              "frame": {
                "title": "% Active Devices by Country (Top 10)",
                "showTitle": true
              },
              "options": {
                "showDataLabels": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}