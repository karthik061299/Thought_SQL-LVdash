{
  "datasets": [
    {
      "name": "hpx_events_model_dataset",
      "displayName": "HPX Events Model Dataset",
      "queryLines": [
        "SELECT geo_country_code, SUM(active_devices_1) as total_active_devices, (SUM(active_devices_1) * 100.0 / SUM(SUM(active_devices_1)) OVER ()) as pct_active_devices_by_country, RANK() OVER (ORDER BY SUM(active_devices_1) DESC) as device_rank FROM team_css_analytics_prod.hpx_analytics.hpx_events_model_iide_metric_view WHERE app_version NOT IN ('not a store release', 'other non-releases') AND event_date_time >= '2023-05-01' AND os_version IS NOT NULL GROUP BY geo_country_code HAVING RANK() OVER (ORDER BY SUM(active_devices_1) DESC) <= 10 ORDER BY SUM(active_devices_1) DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "Active Devices by Country Analysis",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_country_bar",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_model_dataset",
                  "fields": [
                    { "name": "geo_country_code", "expression": "`geo_country_code`" },
                    { "name": "pct_active_devices_by_country", "expression": "`pct_active_devices_by_country`" },
                    { "name": "total_active_devices", "expression": "`total_active_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "geo_country_code",
                  "displayName": "Country",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "pct_active_devices_by_country",
                  "displayName": "% Active Devices by Country",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "% Active Devices by Country (Top 10)",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}