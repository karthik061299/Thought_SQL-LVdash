{
  "datasets": [
    {
      "name": "active_devices_by_country_dataset",
      "displayName": "Active Devices by Country Dataset",
      "queryLines": [
        "SELECT geo_country_code, COUNT(DISTINCT device_id) as active_devices, COUNT(DISTINCT device_id) / SUM(COUNT(DISTINCT device_id)) OVER () as pct_active_devices FROM hpx_events_model WHERE event_date_time >= '2023-05-01' AND app_version NOT IN ('not a store release', 'other non-releases') AND os_version IS NOT NULL GROUP BY geo_country_code ORDER BY active_devices DESC LIMIT 10"
      ]
    }
  ],
  "pages": [
    {
      "name": "active_devices_by_country_page",
      "displayName": "% Active Devices by Country (Top 10)",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_country_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "active_devices_by_country_dataset",
                  "fields": [
                    { "name": "geo_country_code", "expression": "`geo_country_code`" },
                    { "name": "active_devices", "expression": "`active_devices`" },
                    { "name": "pct_active_devices", "expression": "`pct_active_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "geo_country_code",
                  "displayName": "Country",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "pct_active_devices",
                  "displayName": "% Active Devices by Country",
                  "scale": { "type": "quantitative" },
                  "axis": {
                    "format": "PERCENTAGE"
                  }
                }
              },
              "frame": {
                "title": "% Active Devices by Country (Top 10)",
                "showTitle": true
              },
              "options": {
                "dataLabels": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}