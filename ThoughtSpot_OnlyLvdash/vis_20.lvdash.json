{
  "datasets": [
    {
      "name": "active_devices_by_country_dataset",
      "displayName": "Active Devices by Country Dataset",
      "queryLines": [
        "WITH ranked_countries AS (\n  SELECT\n    geo_country_code,\n    SUM(active_devices) as total_active_devices,\n    SUM(active_devices) / SUM(SUM(active_devices)) OVER () as pct_active_devices,\n    RANK() OVER (ORDER BY SUM(active_devices) DESC) as country_rank\n  FROM\n    hpx_events_model\n  WHERE\n    event_date_time >= '2023-05-01'\n    AND app_version NOT IN ('not a store release', 'other non-releases')\n    AND os_version IS NOT NULL\n  GROUP BY\n    geo_country_code\n)\nSELECT\n  geo_country_code,\n  total_active_devices as mau,\n  pct_active_devices,\n  country_rank\nFROM\n  ranked_countries\nWHERE\n  country_rank <= 10\nORDER BY\n  mau DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "active_devices_by_country_page",
      "displayName": "% Active Devices by Country (Top 10)",
      "layout": [
        {
          "widget": {
            "name": "active_devices_by_country_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "active_devices_by_country_dataset",
                  "fields": [
                    { "name": "geo_country_code", "expression": "`geo_country_code`" },
                    { "name": "pct_active_devices", "expression": "`pct_active_devices`" },
                    { "name": "mau", "expression": "`mau`" },
                    { "name": "country_rank", "expression": "`country_rank`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "geo_country_code",
                  "displayName": "Country",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "pct_active_devices",
                  "displayName": "% Active Devices by Country",
                  "scale": { "type": "quantitative" },
                  "format": { "type": "percent", "decimals": 1 }
                }
              },
              "frame": {
                "title": "% Active Devices by Country (Top 10)",
                "showTitle": true
              },
              "options": {
                "showDataLabels": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}