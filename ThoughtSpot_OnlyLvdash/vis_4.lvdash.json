{
  "datasets": [
    {
      "name": "top_onpress_events_dataset",
      "displayName": "Top OnPress Events Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', event_date_time) as daily_event_date, control, SUM(clicks) as total_clicks FROM hpx_events_model WHERE event_date_time >= '2023-05-01' AND ui_action = 'onpress' AND control NOT LIKE '%testid%' AND control != 'welcome core navigationviewitem id' GROUP BY DATE_TRUNC('DAY', event_date_time), control ORDER BY daily_event_date, total_clicks DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "Top On Press Events",
      "layout": [
        {
          "widget": {
            "name": "top_onpress_stacked_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "top_onpress_events_dataset",
                  "fields": [
                    { "name": "daily_event_date", "expression": "`daily_event_date`" },
                    { "name": "control", "expression": "`control`" },
                    { "name": "total_clicks", "expression": "`total_clicks`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "total_clicks",
                  "displayName": "Total Clicks",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "control",
                  "displayName": "Control",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "Top OnPress Events by Control Over Time",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}