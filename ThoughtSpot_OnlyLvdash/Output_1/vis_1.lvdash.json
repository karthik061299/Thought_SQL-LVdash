{
  "datasets": [
    {
      "name": "dataset_1",
      "displayName": "Active Devices Dataset",
      "asset_name": "workspace.default.hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "Active Devices Visualization",
      "layout": [
        {
          "widget": {
            "name": "widget_1",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_1",
                  "fields": [
                    {
                      "name": "daily_event_date",
                      "expression": "DATE_TRUNC('DAY', `Event Date Time`)"
                    },
                    {
                      "name": "active_devices",
                      "expression": "MEASURE(`Active Devices`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "scale": {
                    "type": "temporal"
                  },
                  "displayName": "Daily Event Date"
                },
                "y": {
                  "fieldName": "active_devices",
                  "scale": {
                    "type": "quantitative"
                  },
                  "displayName": "Active Devices"
                }
              },
              "frame": {
                "showTitle": true,
                "title": "Active Devices Over Time"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}