{
  "datasets": [
    {
      "name": "hpx_events_model",
      "displayName": "HPX Events Model",
      "asset_name": "workspace.default.hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "viz_3_page",
      "displayName": "New Devices by Top 10 Countries",
      "layout": [
        {
          "widget": {
            "name": "new_devices_by_country_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_model",
                  "fields": [
                    { "name": "geo_country_code", "expression": "`Geo Country Code`" },
                    { "name": "new_devices", "expression": "SUM(`New Devices`)" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 1,
              "widgetType": "pie",
              "encodings": {
                "angle": {
                  "fieldName": "new_devices",
                  "displayName": "New Devices"
                },
                "color": {
                  "fieldName": "geo_country_code",
                  "displayName": "Geo Country Code"
                }
              },
              "frame": {
                "title": "New Devices by Top 10 Countries",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}