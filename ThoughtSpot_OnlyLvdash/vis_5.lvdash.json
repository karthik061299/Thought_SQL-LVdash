{
  "datasets": [
    {
      "name": "unique_screen_views_dataset",
      "displayName": "Unique Screen Views Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', event_date_time) as daily_event_date, screen, SUM(unique_screen_views) as total_unique_screen_views FROM hpx_events_model WHERE ui_action = 'screendisplayed' AND screen NOT IN ('home', 'welcome', 'welcomecontainer', 'welcome-core', 'support') AND event_date_time >= '2023-05-01' GROUP BY DATE_TRUNC('DAY', event_date_time), screen ORDER BY daily_event_date, total_unique_screen_views DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_5",
      "displayName": "Top Unique Screen Views (excl. Home/Welcome)",
      "layout": [
        {
          "widget": {
            "name": "unique_screen_views_stacked_bar",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "unique_screen_views_dataset",
                  "fields": [
                    { "name": "daily_event_date", "expression": "`daily_event_date`" },
                    { "name": "screen", "expression": "`screen`" },
                    { "name": "total_unique_screen_views", "expression": "`total_unique_screen_views`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Monthly Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "total_unique_screen_views",
                  "displayName": "Unique Screen Views",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "screen",
                  "displayName": "Screen",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "Top Unique Screen Views by Screen Over Time",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}