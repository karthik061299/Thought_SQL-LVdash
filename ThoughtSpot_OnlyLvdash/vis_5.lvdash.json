{
  "datasets": [
    {
      "name": "top_unique_screen_views_dataset",
      "displayName": "Top Unique Screen Views Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', event_date_time) as daily_event_date, screen, COUNT(DISTINCT device_id) as unique_screen_views FROM hpx_events_model WHERE event_date_time >= '2023-05-01' AND ui_action = 'screendisplayed' AND screen NOT IN ('home', 'welcome', 'welcomecontainer', 'welcome-core', 'support') GROUP BY DATE_TRUNC('DAY', event_date_time), screen ORDER BY COUNT(DISTINCT device_id) DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "top_unique_screen_views_page",
      "displayName": "Top Unique Screen Views (excl. Home/Welcome)",
      "layout": [
        {
          "widget": {
            "name": "top_unique_screen_views_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "top_unique_screen_views_dataset",
                  "fields": [
                    { "name": "daily_event_date", "expression": "`daily_event_date`" },
                    { "name": "screen", "expression": "`screen`" },
                    { "name": "unique_screen_views", "expression": "`unique_screen_views`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "daily_event_date",
                  "displayName": "Daily Event Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "unique_screen_views",
                  "displayName": "Unique Screen Views",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "screen",
                  "displayName": "Screen",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "Top Unique Screen Views (excl. Home/Welcome)",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}