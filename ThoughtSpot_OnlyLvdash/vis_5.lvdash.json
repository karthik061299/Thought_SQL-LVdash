{
  "version": "1.0",
  "metadata": {
    "name": "Top Unique Screen Views (excl. Home/Welcome)",
    "description": "HPX Events Model with IIDE Table",
    "created_by": "ThoughtSpot Migration Agent",
    "created_at": "2024-01-01T00:00:00Z",
    "tags": ["thoughtspot-migration", "screen-views", "stacked-column"]
  },
  "datasets": [
    {
      "name": "hpx_events_model",
      "query": "SELECT DATE_TRUNC('day', event_date_time) AS day_event_date_time, screen, COUNT(DISTINCT session_id) AS unique_screen_views FROM team_css_analytics_prod.hpx_analytics.hpx_events_model WHERE event_date_time >= '2023-05-01' AND screen NOT IN ('home', 'welcome', 'welcomecontainer', 'welcome-core', 'support') AND ui_action = 'screendisplayed' GROUP BY DATE_TRUNC('day', event_date_time), screen ORDER BY day_event_date_time, unique_screen_views DESC"
    }
  ],
  "widgets": [
    {
      "id": "viz_5_unique_screen_views",
      "type": "stacked_column_chart",
      "title": "Top Unique Screen Views (excl. Home/Welcome)",
      "description": "Daily trend of unique screen views by screen type",
      "position": {
        "x": 0,
        "y": 8,
        "width": 6,
        "height": 4
      },
      "query": {
        "dataset": "hpx_events_model",
        "fields": [
          {
            "name": "day_event_date_time",
            "type": "dimension"
          },
          {
            "name": "screen",
            "type": "dimension"
          },
          {
            "name": "unique_screen_views",
            "type": "measure"
          }
        ]
      },
      "encoding": {
        "x": {
          "fieldName": "day_event_date_time",
          "displayName": "Monthly Event Date",
          "scale": {
            "type": "temporal"
          }
        },
        "y": {
          "fieldName": "unique_screen_views",
          "displayName": "Unique Screen Views",
          "scale": {
            "type": "quantitative"
          }
        },
        "color": {
          "fieldName": "screen",
          "displayName": "Screen",
          "scale": {
            "type": "categorical"
          }
        }
      },
      "visualization": {
        "type": "stacked_column",
        "properties": {
          "showLegend": true,
          "colors": ["#FF8142", "#56D3A8", "#06BF7F", "#FCD977", "#FCC838", "#82DFE9", "#48D1E0", "#71A1F4", "#BCBCBC", "#0096D3"]
        }
      }
    }
  ],
  "layout": {
    "type": "grid",
    "columns": 12,
    "rows": 8
  },
  "filters": [
    {
      "field": "event_date_time",
      "operator": ">=",
      "value": "2023-05-01"
    },
