{
  "datasets": [
    {
      "name": "home_tile_primary_ctr_dataset",
      "displayName": "Home Tile Primary CTR Dataset",
      "queryLines": [
        "SELECT SUM(unique_interstitial_primary_button_clicks) / SUM(unique_interstitial_views) as ctr_primary FROM hpx_events_model_homepage_summary"
      ]
    }
  ],
  "pages": [
    {
      "name": "home_tile_primary_ctr_page",
      "displayName": "Home Tile Primary CTR",
      "layout": [
        {
          "widget": {
            "name": "home_tile_primary_ctr_counter",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "home_tile_primary_ctr_dataset",
                  "fields": [
                    { "name": "ctr_primary", "expression": "`ctr_primary`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "ctr_primary",
                  "displayName": "Primary CTR",
                  "scale": { "type": "quantitative" },
                  "format": "PERCENTAGE"
                }
              },
              "frame": {
                "title": "Home Tile Primary CTR",
                "showTitle": true,
                "subtitle": "Primary Button Click-Through Rate (Primary Clicks / Interstitial Views)"
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 6, "height": 4 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}