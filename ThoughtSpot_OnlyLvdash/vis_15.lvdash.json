{
  "datasets": [
    {
      "name": "home_tile_views_dataset",
      "displayName": "Home Tile Views Dataset",
      "queryLines": [
        "SELECT home_tile, SUM(unique_tile_views) as total_unique_tile_views FROM hpx_events_model_homepage_summary GROUP BY home_tile ORDER BY SUM(unique_tile_views) DESC LIMIT 1"
      ]
    }
  ],
  "pages": [
    {
      "name": "home_tile_views_page",
      "displayName": "Home Tile Views",
      "layout": [
        {
          "widget": {
            "name": "home_tile_views_counter",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "home_tile_views_dataset",
                  "fields": [
                    { "name": "home_tile", "expression": "`home_tile`" },
                    { "name": "total_unique_tile_views", "expression": "`total_unique_tile_views`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_unique_tile_views",
                  "displayName": "Unique Tile Views",
                  "scale": { "type": "quantitative" }
                },
                "subtitle": {
                  "fieldName": "home_tile",
                  "displayName": "Home Tile"
                }
              },
              "frame": {
                "title": "Most Viewed Home Tile",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 4 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}