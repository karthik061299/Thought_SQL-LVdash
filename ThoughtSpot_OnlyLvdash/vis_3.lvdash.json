{
  "datasets": [
    {
      "name": "new_devices_by_country_dataset",
      "displayName": "New Devices by Country Dataset",
      "queryLines": [
        "SELECT geo_country_code, SUM(new_devices) as total_new_devices FROM hpx_events_model GROUP BY geo_country_code ORDER BY total_new_devices DESC LIMIT 10"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_3",
      "displayName": "New Devices by Top 10 Countries",
      "layout": [
        {
          "widget": {
            "name": "new_devices_by_country_pie_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "new_devices_by_country_dataset",
                  "fields": [
                    { "name": "geo_country_code", "expression": "`geo_country_code`" },
                    { "name": "total_new_devices", "expression": "`total_new_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pie",
              "encodings": {
                "angle": {
                  "fieldName": "total_new_devices",
                  "displayName": "New Devices",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "geo_country_code",
                  "displayName": "Geo Country Code",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "New Devices by Top 10 Countries",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}