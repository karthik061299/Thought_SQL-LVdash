{
  "datasets": [
    {
      "name": "hpx_events_model_dataset",
      "displayName": "HPX Events Model Dataset",
      "queryLines": [
        "SELECT geo_country_code AS geo_country_code, SUM(new_devices_1) AS new_devices FROM team_css_analytics_prod.hpx_analytics.hpx_events_model_iide_metric_view GROUP BY geo_country_code ORDER BY new_devices DESC LIMIT 10"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "New Devices by Top 10 Countries",
      "layout": [
        {
          "widget": {
            "name": "new_devices_pie_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "hpx_events_model_dataset",
                  "fields": [
                    { "name": "geo_country_code", "expression": "`geo_country_code`" },
                    { "name": "new_devices", "expression": "`new_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pie",
              "encodings": {
                "x": {
                  "fieldName": "geo_country_code",
                  "displayName": "Geo Country Code",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "new_devices",
                  "displayName": "New Devices",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "New Devices by Top 10 Countries",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}