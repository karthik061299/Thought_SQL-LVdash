{
  "datasets": [
    {
      "name": "new_devices_by_country_dataset",
      "displayName": "New Devices by Country Dataset",
      "queryLines": [
        "WITH ranked_countries AS (\n  SELECT\n    geo_country_code,\n    COUNT(DISTINCT CASE WHEN is_new_device = true THEN device_id END) as new_devices,\n    RANK() OVER (ORDER BY COUNT(DISTINCT CASE WHEN is_new_device = true THEN device_id END) DESC) as country_rank\n  FROM hpx_events_model\n  GROUP BY geo_country_code\n)\nSELECT\n  CASE\n    WHEN country_rank <= 10 THEN geo_country_code\n    ELSE 'Other'\n  END as country,\n  SUM(new_devices) as new_devices\nFROM ranked_countries\nGROUP BY CASE WHEN country_rank <= 10 THEN geo_country_code ELSE 'Other' END\nORDER BY new_devices DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "new_devices_by_country_page",
      "displayName": "New Devices by Top 10 Countries",
      "layout": [
        {
          "widget": {
            "name": "new_devices_by_country_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "new_devices_by_country_dataset",
                  "fields": [
                    { "name": "country", "expression": "`country`" },
                    { "name": "new_devices", "expression": "`new_devices`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pie",
              "encodings": {
                "color": {
                  "fieldName": "country",
                  "displayName": "Country",
                  "scale": { "type": "categorical" }
                },
                "angle": {
                  "fieldName": "new_devices",
                  "displayName": "New Devices",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "New Devices by Top 10 Countries",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}