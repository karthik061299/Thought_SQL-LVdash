{
  "datasets": [
    {
      "name": "customer_details_dataset",
      "displayName": "Customer Details Dataset",
      "queryLines": [
        "SELECT customer_name, customer_email, total_orders, last_order_date, customer_lifetime_value FROM customer_schema.customer_summary_view ORDER BY customer_lifetime_value DESC LIMIT 100"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_5",
      "displayName": "Customer Details Report",
      "layout": [
        {
          "widget": {
            "name": "customer_table_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "customer_details_dataset",
                  "fields": [
                    { "name": "customer_name", "expression": "`customer_name`" },
                    { "name": "customer_email", "expression": "`customer_email`" },
                    { "name": "total_orders", "expression": "`total_orders`" },
                    { "name": "last_order_date", "expression": "`last_order_date`" },
                    { "name": "customer_lifetime_value", "expression": "`customer_lifetime_value`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "table",
              "encodings": {
                "columns": [
                  {
                    "fieldName": "customer_name",
                    "displayName": "Customer Name",
                    "scale": { "type": "categorical" }
                  },
                  {
                    "fieldName": "customer_email",
                    "displayName": "Email",
                    "scale": { "type": "categorical" }
                  },
                  {
                    "fieldName": "total_orders",
                    "displayName": "Total Orders",
                    "scale": { "type": "quantitative" }
                  },
                  {
                    "fieldName": "last_order_date",
                    "displayName": "Last Order Date",
                    "scale": { "type": "temporal" }
                  },
                  {
                    "fieldName": "customer_lifetime_value",
                    "displayName": "Lifetime Value",
                    "scale": { "type": "quantitative" }
                  }
                ]
              },
              "frame": {
                "title": "Top Customers by Lifetime Value",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 10 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": [
        {
          "fieldName": "last_order_date",
          "displayName": "Last Order Date",
          "type": "FILTER",
          "isMandatory": false,
          "isSingleValue": false
        }
      ]
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}