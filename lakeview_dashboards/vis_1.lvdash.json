{
  "datasets": [
    {
      "name": "website_analytics_dataset",
      "displayName": "Website Analytics Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', visit_date) as daily_visits, SUM(page_views) as total_page_views, SUM(unique_visitors) as total_unique_visitors FROM analytics_schema.website_metrics_view GROUP BY DATE_TRUNC('DAY', visit_date) ORDER BY daily_visits"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "Website Traffic Overview",
      "layout": [
        {
          "widget": {
            "name": "traffic_trend_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "website_analytics_dataset",
                  "fields": [
                    { "name": "daily_visits", "expression": "`daily_visits`" },
                    { "name": "total_page_views", "expression": "`total_page_views`" },
                    { "name": "total_unique_visitors", "expression": "`total_unique_visitors`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "daily_visits",
                  "displayName": "Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fields": [
                    {
                      "fieldName": "total_page_views",
                      "displayName": "Page Views"
                    },
                    {
                      "fieldName": "total_unique_visitors",
                      "displayName": "Unique Visitors"
                    }
                  ],
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Daily Website Traffic Trends",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}