{
  "datasets": [
    {
      "name": "product_performance_dataset",
      "displayName": "Product Performance Dataset",
      "queryLines": [
        "SELECT product_name, category, sales_amount, profit_margin, units_sold, last_updated FROM product_schema.performance_metrics_view ORDER BY sales_amount DESC LIMIT 100"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_6",
      "displayName": "Product Performance Table",
      "layout": [
        {
          "widget": {
            "name": "product_performance_table",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "product_performance_dataset",
                  "fields": [
                    { "name": "product_name", "expression": "`product_name`" },
                    { "name": "category", "expression": "`category`" },
                    { "name": "sales_amount", "expression": "`sales_amount`" },
                    { "name": "profit_margin", "expression": "`profit_margin`" },
                    { "name": "units_sold", "expression": "`units_sold`" },
                    { "name": "last_updated", "expression": "`last_updated`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "table",
              "encodings": {
                "columns": [
                  {
                    "fieldName": "product_name",
                    "displayName": "Product Name",
                    "scale": { "type": "categorical" }
                  },
                  {
                    "fieldName": "category",
                    "displayName": "Category",
                    "scale": { "type": "categorical" }
                  },
                  {
                    "fieldName": "sales_amount",
                    "displayName": "Sales Amount",
                    "scale": { "type": "quantitative" }
                  },
                  {
                    "fieldName": "profit_margin",
                    "displayName": "Profit Margin",
                    "scale": { "type": "quantitative" }
                  },
                  {
                    "fieldName": "units_sold",
                    "displayName": "Units Sold",
                    "scale": { "type": "quantitative" }
                  },
                  {
                    "fieldName": "last_updated",
                    "displayName": "Last Updated",
                    "scale": { "type": "temporal" }
                  }
                ]
              },
              "frame": {
                "title": "Product Performance Summary Table",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 10 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}