{
  "datasets": [
    {
      "name": "kpi_dataset",
      "displayName": "KPI Metrics Dataset",
      "queryLines": [
        "SELECT SUM(total_orders) as total_order_count, AVG(customer_satisfaction) as avg_satisfaction FROM kpi_schema.kpi_view"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_4",
      "displayName": "Key Performance Indicators",
      "layout": [
        {
          "widget": {
            "name": "kpi_counter_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "kpi_dataset",
                  "fields": [
                    { "name": "total_order_count", "expression": "`total_order_count`" },
                    { "name": "avg_satisfaction", "expression": "`avg_satisfaction`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_order_count",
                  "displayName": "Total Orders",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Total Order Count",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 6, "height": 4 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}