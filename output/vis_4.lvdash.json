{
  "datasets": [
    {
      "name": "market_share_dataset",
      "displayName": "Market Share Dataset",
      "queryLines": [
        "SELECT product_line, SUM(market_share_percentage) as total_market_share FROM market_schema.market_share_view GROUP BY product_line ORDER BY total_market_share DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_4",
      "displayName": "Market Share Analysis",
      "layout": [
        {
          "widget": {
            "name": "market_share_pie",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "market_share_dataset",
                  "fields": [
                    { "name": "product_line", "expression": "`product_line`" },
                    { "name": "total_market_share", "expression": "`total_market_share`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pie",
              "encodings": {
                "angle": {
                  "fieldName": "total_market_share",
                  "displayName": "Market Share %",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "product_line",
                  "displayName": "Product Line",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "Market Share Distribution by Product Line",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 8, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}