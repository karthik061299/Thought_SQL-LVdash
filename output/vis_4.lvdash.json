{
  "datasets": [
    {
      "name": "orders_dataset",
      "displayName": "Orders Dataset",
      "queryLines": [
        "SELECT SUM(total_orders) as total_orders FROM orders_schema.orders_view"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_4",
      "displayName": "KPI Dashboard",
      "layout": [
        {
          "widget": {
            "name": "counter_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "orders_dataset",
                  "fields": [
                    { "name": "total_orders", "expression": "`total_orders`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_orders",
                  "displayName": "Total Orders",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Total Orders KPI",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 6, "height": 4 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}