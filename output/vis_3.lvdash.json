{
  "datasets": [
    {
      "name": "region_dataset",
      "displayName": "Regional Sales Dataset",
      "queryLines": [
        "SELECT region, SUM(sales_amount) as aggregated_sales FROM regional_schema.regional_view GROUP BY region ORDER BY aggregated_sales DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_3",
      "displayName": "Regional Distribution",
      "layout": [
        {
          "widget": {
            "name": "region_pie_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "region_dataset",
                  "fields": [
                    { "name": "region", "expression": "`region`" },
                    { "name": "aggregated_sales", "expression": "`aggregated_sales`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pie",
              "encodings": {
                "angle": {
                  "fieldName": "aggregated_sales",
                  "displayName": "Sales Amount",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "region",
                  "displayName": "Region",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "Sales Distribution by Region",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}