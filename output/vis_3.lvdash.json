{
  "datasets": [
    {
      "name": "kpi_metrics_dataset",
      "displayName": "KPI Metrics Dataset",
      "queryLines": [
        "SELECT SUM(revenue) as total_revenue, COUNT(DISTINCT customer_id) as unique_customers, AVG(order_value) as avg_order_value FROM business_schema.metrics_view"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_3",
      "displayName": "KPI Dashboard",
      "layout": [
        {
          "widget": {
            "name": "revenue_kpi",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "kpi_metrics_dataset",
                  "fields": [
                    { "name": "total_revenue", "expression": "`total_revenue`" },
                    { "name": "unique_customers", "expression": "`unique_customers`" },
                    { "name": "avg_order_value", "expression": "`avg_order_value`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "counter",
              "encodings": {
                "value": {
                  "fieldName": "total_revenue",
                  "displayName": "Total Revenue",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Total Revenue KPI",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 4, "height": 4 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}