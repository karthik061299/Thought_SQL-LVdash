{
  "datasets": [
    {
      "name": "revenue_distribution_dataset",
      "displayName": "Revenue Distribution Dataset",
      "queryLines": [
        "SELECT product_category, SUM(revenue) as sum_revenue FROM example_schema.revenue_view GROUP BY product_category ORDER BY sum_revenue DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "revenue_page",
      "displayName": "Revenue",
      "layout": [
        {
          "widget": {
            "name": "revenue_distribution_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "revenue_distribution_dataset",
                  "fields": [
                    { "name": "product_category", "expression": "`product_category`" },
                    { "name": "sum_revenue", "expression": "`sum_revenue`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pie",
              "encodings": {
                "angle": {
                  "fieldName": "sum_revenue",
                  "displayName": "Revenue",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "product_category",
                  "displayName": "Product Category",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "Revenue Distribution",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}