{
  "datasets": [
    {
      "name": "flow_dataset",
      "displayName": "Customer Journey Flow Dataset",
      "queryLines": [
        "SELECT stage1, stage2, stage3, SUM(flow_count) as total_flow FROM flow_schema.customer_journey_view GROUP BY stage1, stage2, stage3 ORDER BY total_flow DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_5",
      "displayName": "Customer Journey Flow",
      "layout": [
        {
          "widget": {
            "name": "journey_sankey_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "flow_dataset",
                  "fields": [
                    { "name": "stage1", "expression": "`stage1`" },
                    { "name": "stage2", "expression": "`stage2`" },
                    { "name": "stage3", "expression": "`stage3`" },
                    { "name": "total_flow", "expression": "`total_flow`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 1,
              "widgetType": "sankey",
              "encodings": {
                "value": {
                  "fieldName": "total_flow",
                  "displayName": "Flow Count"
                },
                "stages": [
                  {
                    "fieldName": "stage1",
                    "displayName": "Initial Stage"
                  },
                  {
                    "fieldName": "stage2",
                    "displayName": "Middle Stage"
                  },
                  {
                    "fieldName": "stage3",
                    "displayName": "Final Stage"
                  }
                ]
              },
              "frame": {
                "title": "Customer Journey Flow Analysis",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}