{
  "datasets": [
    {
      "name": "sales_performance_dataset",
      "displayName": "Sales Performance Dataset",
      "queryLines": [
        "SELECT region, product_category, SUM(sales_amount) as total_sales, SUM(units_sold) as total_units FROM sales_schema.sales_metrics_view GROUP BY region, product_category ORDER BY region, product_category"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_2",
      "displayName": "Sales Performance Dashboard",
      "layout": [
        {
          "widget": {
            "name": "sales_bar_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_performance_dataset",
                  "fields": [
                    { "name": "region", "expression": "`region`" },
                    { "name": "product_category", "expression": "`product_category`" },
                    { "name": "total_sales", "expression": "`total_sales`" },
                    { "name": "total_units", "expression": "`total_units`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "region",
                  "displayName": "Region",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "total_sales",
                  "displayName": "Total Sales",
                  "scale": { "type": "quantitative" }
                },
                "color": {
                  "fieldName": "product_category",
                  "displayName": "Product Category",
                  "scale": { "type": "categorical" }
                }
              },
              "frame": {
                "title": "Sales Performance by Region and Product Category",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}