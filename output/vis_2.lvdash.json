{
  "datasets": [
    {
      "name": "product_dataset",
      "displayName": "Product Performance Dataset",
      "queryLines": [
        "SELECT product_category, SUM(revenue) as total_revenue, AVG(profit_margin) as avg_profit_margin FROM product_schema.product_view GROUP BY product_category ORDER BY total_revenue DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_2",
      "displayName": "Product Performance",
      "layout": [
        {
          "widget": {
            "name": "product_bar_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "product_dataset",
                  "fields": [
                    { "name": "product_category", "expression": "`product_category`" },
                    { "name": "total_revenue", "expression": "`total_revenue`" },
                    { "name": "avg_profit_margin", "expression": "`avg_profit_margin`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "product_category",
                  "displayName": "Product Category",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "total_revenue",
                  "displayName": "Total Revenue",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Revenue by Product Category",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}