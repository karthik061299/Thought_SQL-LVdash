{
  "datasets": [
    {
      "name": "sales_by_region_dataset",
      "displayName": "Sales by Region Dataset",
      "queryLines": [
        "SELECT region, SUM(total_sales) as total_sales FROM sales_analytics.regional_sales_view GROUP BY region ORDER BY total_sales DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "sales_page",
      "displayName": "Sales",
      "layout": [
        {
          "widget": {
            "name": "sales_by_region_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_by_region_dataset",
                  "fields": [
                    { "name": "region", "expression": "`region`" },
                    { "name": "total_sales", "expression": "`total_sales`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "region",
                  "displayName": "Region",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "total_sales",
                  "displayName": "Total Sales",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Sales by Region",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}