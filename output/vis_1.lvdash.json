{
  "datasets": [
    {
      "name": "sales_performance_dataset",
      "displayName": "Sales Performance Dataset",
      "queryLines": [
        "SELECT product_category, SUM(sales_amount) as total_sales, COUNT(order_id) as order_count FROM sales_schema.sales_view GROUP BY product_category ORDER BY total_sales DESC"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "Sales Performance Dashboard",
      "layout": [
        {
          "widget": {
            "name": "sales_bar_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_performance_dataset",
                  "fields": [
                    { "name": "product_category", "expression": "`product_category`" },
                    { "name": "total_sales", "expression": "`total_sales`" },
                    { "name": "order_count", "expression": "`order_count`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "bar",
              "encodings": {
                "x": {
                  "fieldName": "product_category",
                  "displayName": "Product Category",
                  "scale": { "type": "categorical" }
                },
                "y": {
                  "fieldName": "total_sales",
                  "displayName": "Total Sales",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Sales Performance by Product Category",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}