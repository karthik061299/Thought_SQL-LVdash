{
  "datasets": [
    {
      "name": "user_activity_dataset",
      "displayName": "User Activity Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', date) as date, SUM(daily_users) as daily_users FROM user_schema.user_activity_view GROUP BY DATE_TRUNC('DAY', date) ORDER BY date"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "User Analytics",
      "layout": [
        {
          "widget": {
            "name": "line_chart_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "user_activity_dataset",
                  "fields": [
                    { "name": "date", "expression": "`date`" },
                    { "name": "daily_users", "expression": "`daily_users`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "date",
                  "displayName": "Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fieldName": "daily_users",
                  "displayName": "Daily Users",
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Daily Users Over Time",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}