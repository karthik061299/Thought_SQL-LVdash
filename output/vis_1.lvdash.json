{
  "datasets": [
    {
      "name": "daily_users_dataset",
      "displayName": "Daily Users Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('DAY', date) as daily_date, SUM(daily_users) as total_daily_users, SUM(SUM(daily_users)) OVER (ORDER BY DATE_TRUNC('DAY', date) ROWS UNBOUNDED PRECEDING) as cumulative_users FROM example_schema.user_activity_table GROUP BY DATE_TRUNC('DAY', date) ORDER BY daily_date"
      ]
    }
  ],
  "pages": [
    {
      "name": "overview_page",
      "displayName": "Overview",
      "layout": [
        {
          "widget": {
            "name": "daily_user_activity_widget",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "daily_users_dataset",
                  "fields": [
                    { "name": "daily_date", "expression": "`daily_date`" },
                    { "name": "total_daily_users", "expression": "`total_daily_users`" },
                    { "name": "cumulative_users", "expression": "`cumulative_users`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "daily_date",
                  "displayName": "Date",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fields": [
                    {
                      "fieldName": "total_daily_users",
                      "displayName": "Daily Users"
                    },
                    {
                      "fieldName": "cumulative_users",
                      "displayName": "Cumulative Users"
                    }
                  ],
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Daily User Activity",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}