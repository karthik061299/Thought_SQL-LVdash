{
  "datasets": [
    {
      "name": "sales_dataset",
      "displayName": "Sales Dataset",
      "queryLines": [
        "SELECT DATE_TRUNC('MONTH', order_date) as month_date, SUM(sales_amount) as total_sales, COUNT(order_id) as order_count FROM sales_schema.sales_view GROUP BY DATE_TRUNC('MONTH', order_date) ORDER BY month_date"
      ]
    }
  ],
  "pages": [
    {
      "name": "page_1",
      "displayName": "Sales Overview",
      "layout": [
        {
          "widget": {
            "name": "sales_line_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "sales_dataset",
                  "fields": [
                    { "name": "month_date", "expression": "`month_date`" },
                    { "name": "total_sales", "expression": "`total_sales`" },
                    { "name": "order_count", "expression": "`order_count`" }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "line",
              "encodings": {
                "x": {
                  "fieldName": "month_date",
                  "displayName": "Month",
                  "scale": { "type": "temporal" }
                },
                "y": {
                  "fields": [
                    {
                      "fieldName": "total_sales",
                      "displayName": "Total Sales"
                    },
                    {
                      "fieldName": "order_count",
                      "displayName": "Order Count"
                    }
                  ],
                  "scale": { "type": "quantitative" }
                }
              },
              "frame": {
                "title": "Monthly Sales and Order Trends",
                "showTitle": true
              }
            }
          },
          "position": { "x": 0, "y": 0, "width": 12, "height": 8 }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}