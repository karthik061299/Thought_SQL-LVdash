{
  "datasets": [
    {
      "name": "dataset_3",
      "displayName": "New Devices by Country Dataset",
      "asset_name": "workspace.default.hpx_events_model"
    }
  ],
  "pages": [
    {
      "name": "page_3",
      "displayName": "New Devices by Top 10 Countries",
      "layout": [
        {
          "widget": {
            "name": "new_devices_pie_chart",
            "queries": [
              {
                "name": "main_query",
                "query": {
                  "datasetName": "dataset_3",
                  "fields": [
                    {
                      "name": "geo_country_code",
                      "expression": "`Geo Country Code`"
                    },
                    {
                      "name": "new_devices",
                      "expression": "SUM(`New Devices`)"
                    }
                  ],
                  "disaggregated": false
                }
              }
            ],
            "spec": {
              "version": 3,
              "widgetType": "pie",
              "encodings": {
                "angle": {
                  "fieldName": "new_devices",
                  "displayName": "New Devices"
                },
                "color": {
                  "fieldName": "geo_country_code",
                  "displayName": "Country"
                }
              },
              "frame": {
                "showTitle": true,
                "title": "New Devices by Country"
              }
            }
          },
          "position": {
            "x": 0,
            "y": 0,
            "width": 12,
            "height": 8
          }
        }
      ],
      "pageType": "PAGE_TYPE_CANVAS",
      "filters": []
    }
  ],
  "uiSettings": {
    "theme": {
      "widgetHeaderAlignment": "ALIGNMENT_UNSPECIFIED"
    }
  }
}